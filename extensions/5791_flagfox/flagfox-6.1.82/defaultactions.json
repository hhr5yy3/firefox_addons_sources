},{
"name":"My Info",
"template":"https://iplookup.flagfox.net/",
"show":true
},{
"name":"is.gd URL",
"template":"https://is.gd/create.php?url={fullURL}"
},{
"name":"Copy IP",
"template":"copystring:{IPaddress}",
"show":true
},{
"name":"Copy Server Info",
