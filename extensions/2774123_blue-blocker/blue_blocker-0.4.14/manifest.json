{
  "name": "Blue Blocker",
  "description": "Blocks all Twitter Blue verified users on twitter.com",
  "version": "0.4.14",
  "manifest_version": 2,
  "icons": {
    "128": "icon/icon-128.png"
  },
  "background": {
    "scripts": [
      "service-worker-loader.js"
    ],
    "type": "module"
  },
  "content_scripts": [],
  "permissions": [
    "storage",
    "unlimitedStorage",
    "*://*.twitter.com/*",
    "*://twitter.com/*",
    "*://*.x.com/*",
    "*://x.com/*"
  ],
  "web_accessible_resources": [
    "src/injected/*",
    "icon/*",
    "pages/consent/*",
    "assets/chunk-b39a70df.js",
    "assets/chunk-70aa1672.js",
    "assets/chunk-4da8f9f7.js",
    "assets/chunk-07136654.js",
    "assets/chunk-e8076b22.js",
    "assets/chunk-529bb964.js"
  ],
  "browser_action": {
    "default_popup": "src/popup/index.html",
    "default_icon": "icon/icon-128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{119be3f3-597c-4f6a-9caf-627ee431d374}"
    }
  }
}