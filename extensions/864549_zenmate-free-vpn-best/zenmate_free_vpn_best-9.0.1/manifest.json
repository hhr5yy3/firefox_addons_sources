{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "short_name": "ZenMate VPN",
  "description": "__MSG_appDesc__",
  "version": "9.0.1",
  "icons": {
    "16": "icons/standard-16.png",
    "48": "icons/standard-48.png",
    "64": "icons/standard-64.png",
    "128": "icons/standard-128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/standard-16.png",
      "48": "icons/standard-48.png",
      "64": "icons/standard-64.png",
      "128": "icons/standard-128.png"
    },
    "default_popup": "index.html",
    "default_title": "ZenMate"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.zenmate.com/*",
        "*://*.zenmate.io/*"
      ],
      "js": [
        "page_api.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [
      "assets/libs/crypto.js",
      "background.js",
      "runtime.js"
    ]
  },
  "permissions": [
    "*://*/*",
    "tabs",
    "webRequest",
    "privacy",
    "webRequestBlocking",
    "proxy",
    "unlimitedStorage",
    "storage",
    "notifications",
    "cookies",
    "alarms",
    "browsingData",
    "webNavigation"
  ],
  "web_accessible_resources": [
    "assets/css/*"
  ],
  "default_locale": "en",
  "applications": {
    "gecko": {
      "strict_min_version": "91.1.0",
      "id": "firefox-webext@zenmate.com"
    }
  }
}