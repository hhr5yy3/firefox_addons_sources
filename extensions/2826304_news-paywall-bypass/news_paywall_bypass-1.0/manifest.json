{
  "manifest_version": 2,
  "name": "News Paywall Bypass",
  "version": "1.0",
  "description": "News Paywall Bypass",
  "icons": {
    "48": "icons/news-48.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "webRequest"
  ],
  "browser_action": {
    "default_icon": "icons/news-48.png",
    "default_title": "News Paywall Bypass",
    "default_popup": "popup/bypass.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "newspass@newspass.com",
      "strict_min_version": "42.0"
    },
    "safari": {
      "strict_min_version": "14"
    }
  }
}