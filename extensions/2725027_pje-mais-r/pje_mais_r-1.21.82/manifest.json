{
  "manifest_version": 2,
  "name": "PJe+R",
  "version": "1.21.82",
  "description": "Extens\u00e3o com melhorias na utiliza\u00e7\u00e3o do PJe",
  "author": "Ageilson Rodrigues da Silva & Caio Moyses de Lima & Elmo de Oliveira de Moraes & Igor Andr\u00e9 Madeira Oliveira & Israel Azevedo Fabiano &  Sisenando Gomes Calixto de Sousa &  Vitor Rolemberg Guerra Costa",
  "browser_action": {
    "default_popup": "./popup/index.html",
    "default_icon": "logo.svg",
    "default_title": "PJe +R"
  },
  "options_ui": {
    "page": "./options/index.html",
    "open_in_tab": true,
    "chrome_style": false
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "./vue.manifesto.js",
        "./editor/AppEditor.js",
        "./content/index.global.js"
      ],
      "all_frames": true,
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "icons": {
    "128": "logo.svg"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "webNavigation",
    "tabs",
    "*://*/"
  ],
  "web_accessible_resources": [
    "content/style.css",
    "pjecomm.js",
    "CacheXMLHttpRequestProxy.js"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{0c409df1-95dc-411a-b4bf-e6063635f80f}"
    }
  }
}