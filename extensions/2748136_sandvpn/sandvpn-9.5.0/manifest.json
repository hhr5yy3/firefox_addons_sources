{
  "browser_specific_settings": {
    "gecko": {
      "id": "sandvpn_@sandvpn.com",
      "strict_min_version": "91.1.0"
    }
  },
  "name": "SandVPN",
  "description": "Fast, Secure, Stable",
  "version": "9.5.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "SandVPN"
  },
  "icons": {
    "192": "logo192.png"
  },
  "permissions": [
    "identity",
    "webNavigation",
    "cookies",
    "browsingData",
    "notifications",
    "tabs",
    "privacy",
    "proxy",
    "management",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "activeTab",
    "contextMenus",
    "clipboardWrite",
    "alarms"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "logo192.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end",
      "match_about_blank": true
    }
  ]
}