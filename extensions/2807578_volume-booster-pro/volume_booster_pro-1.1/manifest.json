{
  "name": "Volume Booster",
  "description": "Boost your volume up to 500 %",
  "version": "1.1",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "https://*/*",
    "http://*/*"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{2c5dc68a-aeca-4c36-84c7-324994d4dadc}"
    }
  }
}