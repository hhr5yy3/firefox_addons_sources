{
  "manifest_version": 2,
  "name": "WebToEpub",
  "version": "1.0.1.0",
  "default_locale": "en",
  "icons": {
    "128": "book128.png"
  },
  "permissions": [
    "cookies",
    "downloads",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "unlimitedStorage",
    "<all_urls>"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "WebToEpub@Baka-tsuki.org",
      "strict_min_version": "109.0"
    }
  },
  "minimum_chrome_version": "119",
  "browser_action": {
    "browser_style": false,
    "default_title": "",
    "default_icon": "book128.png",
    "default_popup": "popup.html"
  }
}