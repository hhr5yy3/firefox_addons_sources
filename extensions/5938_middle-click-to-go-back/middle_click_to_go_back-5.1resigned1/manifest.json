{
  "name": "Middle Click To Go Back",
  "version": "5.1resigned1",
  "manifest_version": 2,
  "description": "Go back with your middle mouse button.",
  "icons": {
    "48": "back48.png",
    "128": "back128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*"
      ],
      "all_frames": true,
      "js": [
        "mctgb.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "MiddleClickToGoBack@lembas"
    }
  }
}