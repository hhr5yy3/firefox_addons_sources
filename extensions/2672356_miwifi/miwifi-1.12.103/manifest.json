{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "1.12.103",
  "default_locale": "ru",
  "description": "__MSG_extDescription__",
  "short_name": "__MSG_sNm__",
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "permissions": [
    "storage",
    "*://boosty.to/*"
  ],
  "web_accessible_resources": [
    "lang/*.json",
    "img/*.png",
    "img/*.jpg",
    "inc/img/*.jpg",
    "img/*.svg",
    "css/*.css",
    "inc/routers.html"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery.js",
        "js/content.js"
      ]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "css/styles.css"
      ],
      "include_globs": [
        "*://10.*",
        "*://100.*",
        "*://172.*",
        "*://192.*",
        "*://miwifi.com/*",
        "*://boosty.to/*"
      ]
    }
  ],
  "browser_action": {
    "default_title": "RUMIWIFI",
    "default_icon": "img/icon_16.png",
    "default_popup": "popup.html"
  },
  "homepage_url": "https://boosty.to/miwifi",
  "browser_specific_settings": {
    "gecko": {
      "id": "{7840a7c8-14bb-41e6-bb89-fb2f830594a9}"
    }
  }
}