{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "version": "1.15.0",
  "short_name": "RateHawk",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "web_accessible_resources": [
    "assets/icons/*",
    "assets/img/*",
    "assets/fonts/*",
    "css/*"
  ],
  "icons": {
    "16": "assets/icons/icon-16.png",
    "32": "assets/icons/icon-32.png",
    "48": "assets/icons/icon-48.png",
    "128": "assets/icons/icon-128.png"
  },
  "homepage_url": "https://www.ratehawk.com/",
  "permissions": [
    "cookies",
    "history",
    "storage",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "https://cnt.worldota.net/funnel.js",
    "https://api.worldota.net/api/b2b/v3/search/hp/ex",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "author": "RateHawk",
  "browser_action": {
    "default_icon": {
      "16": "assets/icons/icon-16.png",
      "32": "assets/icons/icon-32.png",
      "48": "assets/icons/icon-48.png",
      "128": "assets/icons/icon-128.png"
    },
    "default_title": "RateHawk",
    "browser_style": false
  },
  "background": {
    "scripts": [
      "js/background.bundle.js"
    ]
  }
}