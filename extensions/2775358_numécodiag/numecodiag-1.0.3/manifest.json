{
  "manifest_version": 2,
  "name": "Num\u00c9coDiag",
  "description": "Cette WebExtension d\u00e9velopp\u00e9e et mise \u00e0 disposition gratuitement par la MiNumEco facilite l'auto-diagnostic de l\u2019\u00e9coconception d\u2019un service num\u00e9rique, conform\u00e9ment au R\u00e9f\u00e9rentiel G\u00e9n\u00e9ral d'\u00c9coconception de Services Num\u00e9riques (RGESN)",
  "version": "1.0.3",
  "icons": {
    "16": "./static/numecodiag-logo-16x16.png",
    "48": "./static/numecodiag-logo-48x48.png",
    "128": "./static/numecodiag-logo-128x128.png"
  },
  "browser_action": {
    "default_icon": "./static/numecodiag-logo-48x48.png",
    "default_popup": "./index.html",
    "default_title": "Num\u00c9coDiag"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+D"
      }
    }
  },
  "permissions": [
    "storage"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{0e4491ce-9f5f-4b9b-a5c8-c64dd4e54c1f}"
    }
  }
}