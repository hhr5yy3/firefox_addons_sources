{
  "manifest_version": 2,
  "name": "__MSG_EXTENSION_NAME__",
  "short_name": "Alitools",
  "description": "__MSG_EXTENSION_DESCRIPTION__",
  "default_locale": "en",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "assets/styles/fonts.css",
        "assets/styles/common-react.css",
        "assets/styles/content-all-sites.css"
      ],
      "js": [
        "common-react.js",
        "content-all-sites.js"
      ]
    }
  ],
  "icons": {
    "32": "assets/icons/32_ny.png",
    "128": "assets/icons/128_ny.png"
  },
  "browser_action": {
    "default_icon": "assets/icons/32_ny.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "assets/fonts/*"
  ],
  "homepage_url": "https://alitools.io",
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "cookies",
    "notifications",
    "tabs",
    "webNavigation",
    "storage",
    "webRequest"
  ],
  "version": "24.12.1.17305",
  "browser_specific_settings": {
    "gecko": {
      "id": "{019f5290-6afb-4863-bc31-87cc0b6adb25}"
    }
  }
}