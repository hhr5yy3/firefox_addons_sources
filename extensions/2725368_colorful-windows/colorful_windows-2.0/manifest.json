{
  "manifest_version": 2,
  "name": "Colorful Windows",
  "version": "2.0",
  "description": "Dynamic colorful firefox windows",
  "homepage_url": "https://github.com/DaveDuck321/Colorful-window-theme",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
