{
  "text": {
    "$id": "text",
    "description": "plain text"
  },
  "url": {
    "$id": "url",
    "description": "url string, can be clicked"
  },
  "multiline": {
    "$id": "multiline",
    "description": "multiline text"
  },
  "file": {
    "$id": "file",
    "description": "large binary object, picture or other file"
  },
  "fileRef": {
    "$id": "fileRef",
    "description": "reference to the file field on another record"
  },
  "email": {
    "$id": "email",
    "description": "valid email address"
  },
  "phone": {
    "$id": "phone",
    "description": "numbers and symbols only"
  },
  "name": {
    "$id": "name",
    "description": "multiple fields to capture name",
    "elements": [
      "firstName",
      "lastName",
      "fullName"
    ]
  },
  "address": {
    "$id": "address",
    "description": "multiple fields to capture address",
    "elements": [
      "street1",
      "street2",
      "city",
      "state",
      "zip",
      "country"
    ]
  },
  "addressRef": {
    "$id": "addressRef",
    "description": "reference to the address field on another record"
  },
  "secret": {
    "$id": "secret",
    "description": "the field value is masked"
  },
  "securityQuestion": {
    "$id": "securityQuestion",
    "description": "Security Question and Answer",
    "elements": [
      "Q",
      "A"
    ]
  },
  "otp": {
    "$id": "otp",
    "description": "captures the seed, displays QR code"
  },
  "cardNumber": {
    "$id": "cardNumber",
    "description": "validates as card number, displays card issuer logo"
  },
  "date": {
    "$id": "date",
    "description": "calendar date with validation, stored as unix milliseconds"
  },
  "cardExpirationDate": {
    "$id": "cardExpirationDate",
    "description": "two fields for expiration month and year or single field with a corresponding mask. Stored as MM/YY"
  },
  "bankAccount": {
    "$id": "bankAccount",
    "description": "bank account information",
    "elements": [
      "accountType",
      "routingNumber",
      "accountNumber"
    ]
  },
  "privateKey": {
    "$id": "privateKey",
    "description": "private key in ASN.1 format"
  },
  "bip39": {
    "$id": "bip39",
    "description": "12/18/24 words corresponding to a 16/20/24 bytes random value"
  }
}