{
  "name": "Systemwalker Desktop Keeper/Navi",
  "version": "2.0",
  "description": "Records and controls operations on webpages.",
  "icons": {
    "128": "icons/no07_128x128.bmp"
  },
  "manifest_version": 2,
  "browser_specific_settings": {
    "gecko": {
      "id": "webmonitoring1.8@dtk.fujitsu.com",
      "strict_min_version": "49.0"
    }
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "downloads",
    "scripting",
    "webRequest",
    "webRequestBlocking",
    "nativeMessaging"
  ]
}