{
  "manifest_version": 3,
  "name": "__MSG_name__",
  "author": "Adguard Software Ltd",
  "version": "1.4.1",
  "description": "__MSG_description__",
  "icons": {
    "16": "assets/images/icons/green-16.png",
    "48": "assets/images/icons/green-48.png",
    "128": "assets/images/icons/green-128.png"
  },
  "options_ui": {
    "page": "options-ui.html"
  },
  "permissions": [
    "nativeMessaging",
    "tabs",
    "activeTab",
    "contextMenus",
    "storage",
    "scripting"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "browserassistant@adguard.com",
      "strict_min_version": "109.0"
    }
  },
  "action": {
    "default_icon": {
      "19": "assets/images/icons/green-19.png",
      "38": "assets/images/icons/green-38.png"
    },
    "default_title": "__MSG_name__",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "default_locale": "en"
}