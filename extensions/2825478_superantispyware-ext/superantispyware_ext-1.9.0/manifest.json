{
  "name": "SUPERAntiSpyware Ext",
  "version": "1.9.0",
  "description": "__MSG_manifest_description__",
  "manifest_version": 2,
  "default_locale": "en",
  "permissions": [
    "http://*/",
    "https://*/",
    "search",
    "webRequestBlocking",
    "topSites",
    "history",
    "scripting",
    "storage",
    "tabs",
    "webNavigation",
    "webRequest"
  ],
  "browser_action": {
    "default_popup": "ui/popup/index.html",
    "default_title": "SUPERAntiSpyware Ext"
  },
  "background": {
    "scripts": [
      "background/shared/activity.js",
      "background/shared/categorization.js",
      "background/shared/init.js",
      "background/shared/searchCounter.js",
      "background/shared/siteBlocking.js",
      "background/shared/trackerBlocking.js",
      "background/shared/trackerBlockingMV2.js"
    ],
    "type": "module"
  },
  "chrome_url_overrides": {
    "newtab": "/ui/newtab/index.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Startpage",
      "keyword": "@startpage",
      "favicon_url": "https://www.startpage.com/favicon.ico",
      "encoding": "UTF-8",
      "is_default": true,
      "search_url": "https://www.startpage.com/do/search?segment=startpage.webscannerpro&source=sas&cat=web&wspob=1&query={searchTerms}"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://search.aol.com/aol/search*",
        "https://*.ask.com/web*",
        "https://*.bing.com/search*",
        "https://search.brave.com/search*",
        "https://duckduckgo.com/*",
        "https://*.google.com/search*",
        "https://www.mojeek.com/search*",
        "https://*.onesearch.com/yhs/search*",
        "https://www.qwant.com/*",
        "https://nova.rambler.ru/search*",
        "https://search.seznam.cz/*",
        "https://search.seznam.sk/*",
        "https://*.startpage.com/search*",
        "https://*.startpage.com/sp/search*",
        "https://*.startpage.com/do/dsearch*",
        "https://*.startpage.com/do/search*",
        "https://swisscows.com/*",
        "https://*.yahoo.com/search*",
        "https://search.yahoo.co.jp/search*",
        "https://*.yandex.com/search*",
        "https://*.yandex.ru/search*"
      ],
      "css": [
        "content-scripts/serp.css"
      ]
    },
    {
      "matches": [
        "https://*.startpage.com/search*",
        "https://*.startpage.com/sp/search*",
        "https://*.startpage.com/do/dsearch*",
        "https://*.startpage.com/do/search*"
      ],
      "css": [
        "content-scripts/shared/serps/startpage.css"
      ]
    },
    {
      "matches": [
        "https://search.aol.com/aol/search*"
      ],
      "js": [
        "content-scripts/shared/serps/aol.js"
      ]
    },
    {
      "matches": [
        "https://*.ask.com/web*"
      ],
      "js": [
        "content-scripts/shared/serps/ask.js"
      ]
    },
    {
      "matches": [
        "https://*.bing.com/search*"
      ],
      "js": [
        "content-scripts/shared/serps/bing.js"
      ]
    },
    {
      "matches": [
        "https://search.brave.com/search*"
      ],
      "js": [
        "content-scripts/shared/serps/brave.js"
      ]
    },
    {
      "matches": [
        "https://duckduckgo.com/*"
      ],
      "js": [
        "content-scripts/shared/serps/duckduckgo.js"
      ]
    },
    {
      "matches": [
        "https://*.google.com/search*"
      ],
      "js": [
        "content-scripts/shared/serps/google.js"
      ]
    },
    {
      "matches": [
        "https://www.mojeek.com/search*"
      ],
      "js": [
        "content-scripts/shared/serps/mojeek.js"
      ]
    },
    {
      "matches": [
        "https://*.onesearch.com/yhs/search*"
      ],
      "js": [
        "content-scripts/shared/serps/onesearch.js"
      ]
    },
    {
      "matches": [
        "https://www.qwant.com/*"
      ],
      "js": [
        "content-scripts/shared/serps/qwant.js"
      ]
    },
    {
      "matches": [
        "https://nova.rambler.ru/search*"
      ],
      "js": [
        "content-scripts/shared/serps/rambler.js"
      ]
    },
    {
      "matches": [
        "https://search.seznam.cz/*",
        "https://search.seznam.sk/*"
      ],
      "js": [
        "content-scripts/shared/serps/seznam.js"
      ]
    },
    {
      "matches": [
        "https://*.startpage.com/search*",
        "https://*.startpage.com/sp/search*",
        "https://*.startpage.com/do/dsearch*",
        "https://*.startpage.com/do/search*"
      ],
      "js": [
        "content-scripts/shared/serps/startpage.js",
        "content-scripts/default-search-engine.js"
      ]
    },
    {
      "matches": [
        "https://swisscows.com/*"
      ],
      "js": [
        "content-scripts/shared/serps/swisscows.js"
      ]
    },
    {
      "matches": [
        "https://*.yahoo.com/search*"
      ],
      "js": [
        "content-scripts/shared/serps/yahoo.js"
      ]
    },
    {
      "matches": [
        "https://search.yahoo.co.jp/search*"
      ],
      "js": [
        "content-scripts/shared/serps/yahoojp.js"
      ]
    },
    {
      "matches": [
        "https://*.yandex.com/search*",
        "https://*.yandex.ru/search*"
      ],
      "js": [
        "content-scripts/shared/serps/yandex.js"
      ]
    }
  ],
  "icons": {
    "16": "resources/icons/icon-16.png",
    "32": "resources/icons/icon-32.png",
    "48": "resources/icons/icon-48.png",
    "128": "resources/icons/icon-128.png"
  },
  "web_accessible_resources": [
    "/resources/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "add-on@superantispywareext.com"
    }
  }
}