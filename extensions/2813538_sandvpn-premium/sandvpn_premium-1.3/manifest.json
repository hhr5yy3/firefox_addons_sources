{
  "browser_specific_settings": {
    "gecko": {
      "id": "sandvpn_premium@sandvpn.com",
      "strict_min_version": "91.1.0"
    }
  },
  "name": "SandVPN Premium",
  "description": "Fast, Secure, Stable",
  "version": "1.3",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "SandVPN Premium"
  },
  "icons": {
    "192": "logo192.png"
  },
  "permissions": [
    "webNavigation",
    "cookies",
    "browsingData",
    "notifications",
    "tabs",
    "privacy",
    "proxy",
    "management",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "activeTab",
    "contextMenus",
    "clipboardWrite"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}