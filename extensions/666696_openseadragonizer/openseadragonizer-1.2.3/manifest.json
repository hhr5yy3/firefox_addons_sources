{
    "manifest_version": 2,
    "name": "OpenSeadragonizer",
    "description": "Display big images in OpenSeadragon to enable zoom and pan.",
    "version": "1.2.3",
    "background": {
        "scripts": ["webextension.js"]
    },
    "permissions": [
        "contextMenus"
    ],
    "icons": {
        "16": "logo16.png",
        "48": "logo48.png",
        "128": "logo128.png"
    }
}
