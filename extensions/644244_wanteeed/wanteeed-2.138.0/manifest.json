{
  "manifest_version": 2,
  "name": "wanteeed",
  "description": "__MSG_description__",
  "version": "2.138.0",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "all_frames": false,
      "run_at": "document_end",
      "js": [
        "src/scripts/content.js",
        "src/scripts/comparator.js",
        "node_modules/@wanteeed/vouchertester/dist/index.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "src/scripts/background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "icons/app-16.png",
    "32": "icons/app-32.png",
    "48": "icons/app-48.png",
    "64": "icons/app-64.png",
    "128": "icons/app-128.png",
    "256": "icons/app-256.png",
    "512": "icons/app-512.png",
    "1024": "icons/app-1024.png"
  },
  "default_locale": "fr",
  "web_accessible_resources": [
    "*"
  ],
  "homepage_url": "http://www.wanteeed.com/",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "webNavigation",
    "cookies",
    "alarms",
    "storage",
    "webRequest"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "browser_action": {
    "default_icon": {
      "16": "icons/toolbar-disabled-light-16.png",
      "32": "icons/toolbar-disabled-light-32.png",
      "64": "icons/toolbar-disabled-light-64.png"
    },
    "default_title": "Wanteeed"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{EDB6A15C-5F8C-4531-92FA-98E988CF233C}"
    }
  }
}