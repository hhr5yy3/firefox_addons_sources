{
  "name": "Print Friendly & PDF",
  "description": "__MSG_description__",
  "default_locale": "en",
  "version": "5.3.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "activeTab",
    "contextMenus"
  ],
  "optional_permissions": [
    "*://*/*",
    "webNavigation",
    "https://translate.google.com/"
  ],
  "background": {
    "scripts": [
      "config.js",
      "common.js"
    ]
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "data/icons/printer-fill-dark-16.png",
      "32": "data/icons/printer-fill-dark-32.png",
      "48": "data/icons/printer-fill-dark-48.png"
    }
  },
  "homepage_url": "https://www.printfriendly.com",
  "icons": {
    "16": "data/icons/printer-fill-dark-16.png",
    "32": "data/icons/printer-fill-dark-32.png",
    "48": "data/icons/printer-fill-dark-48.png",
    "64": "data/icons/printer-fill-dark-64.png",
    "128": "data/icons/printer-fill-dark-128.png",
    "256": "data/icons/printer-fill-dark-256.png"
  },
  "options_ui": {
    "page": "data/options/index.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "commands": {
    "_execute_browser_action": {},
    "toggle-reader-view": {
      "description": "Toggle PrintFriendly"
    }
  },
  "web_accessible_resources": [
    "/data/reader/template.html",
    "/data/inject/PfAlgo/algo.html"
  ],
  "applications": {
    "gecko": {
      "id": "jid0-YQz0l1jthOIz179ehuitYAOdBEs@jetpack",
      "strict_min_version": "79.0"
    }
  }
}