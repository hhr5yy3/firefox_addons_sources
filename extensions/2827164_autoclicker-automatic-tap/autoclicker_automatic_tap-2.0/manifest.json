{
  "name": "IO Auto Clicker",
  "description": "Automating Click Events",
  "version": "2.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "/img/icon-128.png",
    "default_popup": "popup.html",
    "default_title": "Auto Clicker"
  },
  "icons": {
    "16": "/img/icon-128.png",
    "32": "/img/icon-128.png",
    "48": "/img/icon-128.png",
    "128": "/img/icon-128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": [
        "js/common.js",
        "js/AutoClicker.js"
      ],
      "css": [
        "css/content.css"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{67794639-d1c6-4b41-a189-4f99f80237d2}"
    }
  }
}