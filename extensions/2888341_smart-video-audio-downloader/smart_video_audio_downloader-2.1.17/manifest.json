{
  "manifest_version": 3,
  "name": "__MSG_ext_name__",
  "version": "2.1.17",
  "default_locale": "en",
  "description": "__MSG_ext_descr__",
  "browser_specific_settings": {
    "gecko": {
      "id": "@smart-video-audio-downloader"
    }
  },
  "permissions": [
    "tabs",
    "activeTab",
    "downloads",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ],
      "run_at": "document_end",
      "match_about_blank": true,
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "img/*.svg",
        "style/modal.css",
        "modal.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "action": {
    "default_icon": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png",
      "64": "img/icon_64.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "64": "img/icon_64.png",
    "128": "img/icon_128.png"
  }
}