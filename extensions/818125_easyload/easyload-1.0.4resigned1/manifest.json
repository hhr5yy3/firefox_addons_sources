{
  "name": "EasyLoad",
  "version": "1.0.4resigned1",
  "manifest_version": 2,
  "description": "Download video or audio files from any site",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "https://easyload.co/*",
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png"
    }
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": false
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{c6485c3a-2ca8-4751-9fc3-c9a852173c83}"
    }
  }
}