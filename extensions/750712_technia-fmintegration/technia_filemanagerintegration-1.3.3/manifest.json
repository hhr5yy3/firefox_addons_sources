{
  "description": "Add-on for integration of Technia File Manager",
  "manifest_version": 2,
  "name": "Technia File Manager Integration",
  "version": "1.3.3",
  "icons": {
    "48": "icons/filemanager-48.png",
    "96": "icons/filemanager-96.png",
    "128": "icons/filemanager-128.png"
  },
  "applications": {
    "gecko": {
      "id": "tvc_filemanager@techniatranscat.com",
      "strict_min_version": "50.0"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "nativeMessaging"
  ]
}