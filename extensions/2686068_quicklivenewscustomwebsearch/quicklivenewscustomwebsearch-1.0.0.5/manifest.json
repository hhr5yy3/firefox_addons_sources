{
  "background": {
    "scripts": [
      "distJs/constants.js",
      "distJs/helper.js",
      "html/js/storageReplacer.js",
      "util.js",
      "background.js",
      "misc.js"
    ]
  },
  "name": "Quick Live News & Custom Web Search",
  "short_name": "Quick Live News",
  "icons": {
    "128": "icons/icon128_v2.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://quicklivenews.com/*"
      ],
      "run_at": "document_start",
      "all_frames": false,
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "description": "Add the Quick Live News & Custom Web Search extension and click the favicon on your toolbar to get access to latest news and Yahoo!\u00ae search.",
  "version": "1.0.0.5",
  "manifest_version": 2,
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "128": "icons/icon128.png"
    },
    "default_title": "Quick Live News & Custom Web Search"
  },
  "permissions": [
    "*://quicklivenews.com/",
    "tabs",
    "storage"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "web search by Yahoo",
      "keyword": "Quick Live News",
      "favicon_url": "icons/icon128_v2.png",
      "search_url": "https://search.yahoo.com/yhs/search?hspart=mnet&hsimp=yhs-001&type=type9040536-spa-6095-84500&param1=6095&param2=84500&p={searchTerms}",
      "suggest_url": "https://search.yahoo.com/sugg/ie?output=fxjson&command={searchTerms}&nResults=10",
      "is_default": true
    }
  },
  "applications": {
    "gecko": {
      "id": "Quick_Live_News_spxMukSrlT@quicklivenews.com"
    }
  }
}