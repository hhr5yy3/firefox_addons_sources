{
  "version": "0.2.10resigned1",
  "manifest_version": 2,
  "short_name": "translator-lite",
  "name": "Google\u2122 Translator Lite",
  "background": {
    "page": "lib/chrome/background.html"
  },
  "description": "a lite and multi-language translator.",
  "web_accessible_resources": [
    "data/content_script/inject.css"
  ],
  "homepage_url": "http://mybrowseraddon.com/translator-lite.html",
  "options_ui": {
    "chrome_style": true,
    "page": "data/options/options.html"
  },
  "permissions": [
    "storage",
    "webRequest",
    "contextMenus",
    "webRequestBlocking",
    "*://*.gstatic.com/*",
    "*://*.translate.google.com/*"
  ],
  "browser_action": {
    "default_popup": "data/popup/popup.html",
    "default_title": "Google\u2122 Translator Lite",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "run_at": "document_start",
      "js": [
        "data/content_script/inject.js"
      ],
      "matches": [
        "*://*.translate.google.com/*"
      ]
    }
  ],
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "jid1-f3mYMbCpz2AZYl@jetpack"
    }
  }
}