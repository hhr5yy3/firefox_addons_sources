{
  "content_scripts": [
    {
      "matches": [
        "https://europe.tradeonlineservices.com/*",
        "https://*.barclays.com/*",
        "https://*.barclays.net/*",
        "https://*.barclays.intranet/*",
        "https://*.barclays.co.uk/*",
        "https://*.barclayswealth.com/*",
        "https://*.barclayscorp.com/*",
        "https://*.barcapint.com/*",
        "https://*.bmap.barclays/*",
        "https://*.barclays-sepadirect.com/*",
        "https://*.cashfaccloud.co.uk/*",
        "https://*.bacs.co.uk/*",
        "https://*.voca.com/*",
        "https://*.voca.co.uk/*",
        "https://*.identrust.com/*",
        "https://*.paygateservice.com/*",
        "https://*.iconnect.aptbacs.co.uk/*",
        "https://*.interbacs.com/*",
        "https://*.interpayweb.co.uk/*",
        "https://*.mosaicsoftware.co.uk/*",
        "https://*.accountis.net/*",
        "https://*.accountis.lan/*",
        "https://www.directdebitnetwork.com/*",
        "https://apsportal.co.uk/*"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "css": [
        "assets/css/main.css",
        "barclays/assets/css/barclays.css"
      ],
      "js": [
        "first.js",
        "externalLibraries/base64.js",
        "externalLibraries/jquery.js",
        "externalLibraries/rasterizeHTML.js",
        "externalLibraries/DOMPurify.js",
        "app_settings.js",
        "barclays/custom.js",
        "modals.js",
        "utility.js",
        "multiPageCtrl.js",
        "certificateTable.js",
        "customScrollbars.js",
        "verificationResults.js",
        "asn1.js",
        "renderer.js",
        "version.js",
        "logger.js",
        "content.js"
      ]
    }
  ],
  "name": "Gemalto Web Signer for Barclays",
  "icons": {
    "128": "barclays/assets/img/logo128.png",
    "48": "barclays/assets/img/logo48.png",
    "16": "barclays/assets/img/logo16.png"
  },
  "web_accessible_resources": [
    "barclays/templates/*",
    "barclays/assets/*",
    "assets/*"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "externalLibraries/base64.js",
      "app_settings.js",
      "barclays/custom.js",
      "utility.js",
      "logger.js",
      "background.js"
    ]
  },
  "applications": {
    "gecko": {
      "strict_min_version": "58.0",
      "id": "websigner_barclays_live@gemalto"
    }
  },
  "version": "2.1.0.51",
  "options_ui": {
    "page": "diagTools/diagTools.html",
    "browser_style": false
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "nativeMessaging",
    "storage",
    "unlimitedStorage"
  ],
  "description": "\u00a9 Thales Group 2024"
}