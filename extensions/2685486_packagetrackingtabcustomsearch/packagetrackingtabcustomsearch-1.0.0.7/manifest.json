{
  "background": {
    "scripts": [
      "distJs/constants.js",
      "distJs/helper.js",
      "html/js/storageReplacer.js",
      "util.js",
      "background.js",
      "misc.js"
    ]
  },
  "name": "Package Tracking Tab & Custom Web Search",
  "short_name": "Package Tracking Tab",
  "icons": {
    "128": "icons/coloured_icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://packagetrackingtab.com/*"
      ],
      "run_at": "document_start",
      "all_frames": false,
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "description": "Add the Package Tracking Tab & Custom Web Search extension and click the icon on your toolbar to get quick-link access to a package tracking site & Yahoo\u00ae search",
  "version": "1.0.0.7",
  "manifest_version": 2,
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "128": "icons/icon128.png"
    },
    "default_title": "Package Tracking Tab & Custom Web Search"
  },
  "permissions": [
    "*://packagetrackingtab.com/",
    "tabs",
    "storage"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "web search by Yahoo",
      "keyword": "Package Tracking Tab",
      "favicon_url": "icons/icon128.png",
      "search_url": "https://search.yahoo.com/yhs/search?hspart=mnet&hsimp=yhs-001&type=type9085796-spa-3537-84480&param1=3537&param2=84480&p={searchTerms}",
      "suggest_url": "https://search.yahoo.com/sugg/ie?output=fxjson&command={searchTerms}&nResults=10",
      "is_default": true
    }
  },
  "applications": {
    "gecko": {
      "id": "Package_Tracking_Tab___Custom_Web_Search_JVVfIFSBVe@packagetrackingtab.com"
    }
  }
}