{
  "name": "PemHeart Web Client Extension",
  "version": "0.2.1",
  "description": "Zarz\u0105dzanie komunikacj\u0105 z oprogramowaniem PemHeart Web Client.",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "nativeMessaging"
  ],
  "author": "Enigma SOI",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ],
      "all_frames": true
    }
  ]
}