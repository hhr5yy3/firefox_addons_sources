{
  "name": "Download with Ant Download Manager",
  "short_name": "antFF",
  "version": "0.5.6",
  "description": "__MSG_description__",
  "homepage_url": "https://antdownloadmanager.com",
  "manifest_version": 2,
  "default_locale": "en",
  "applications": {
    "gecko": {
      "id": "antffw@antdownloadmanager.com",
      "strict_min_version": "56.0"
    }
  },
  "background": {
    "scripts": [
      "background_FFW3.js",
      "popup/jquery-3.2.1.min.js"
    ]
  },
  "icons": {
    "16": "icons/antFF16.png",
    "32": "icons/antFF32.png",
    "48": "icons/antFF48.png",
    "64": "icons/antFF64.png",
    "128": "icons/antFF128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/app19_disabled_x.png",
      "38": "icons/app38_disabled_x.png"
    },
    "browser_style": false,
    "default_popup": "popup/popup.html",
    "default_title": "AntDM"
  },
  "permissions": [
    "<all_urls>",
    "cookies",
    "contextMenus",
    "downloads",
    "tabs",
    "activeTab",
    "webNavigation",
    "nativeMessaging",
    "webRequest",
    "webRequestBlocking",
    "management",
    "storage"
  ]
}