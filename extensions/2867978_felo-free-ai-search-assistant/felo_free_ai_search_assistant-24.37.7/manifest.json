{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "version": "24.37.7",
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "96": "icon/96.png",
    "128": "icon/128.png"
  },
  "default_locale": "en",
  "permissions": [
    "storage",
    "tabs",
    "contextMenus",
    "activeTab",
    "scripting",
    "<all_urls>"
  ],
  "externally_connectable": {
    "matches": [
      "https://*.felo.ai/*",
      "http://*.felo.ai/*"
    ]
  },
  "options_ui": {
    "page": "options.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_idle",
      "js": [
        "content-scripts/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "content-scripts/content.css"
  ],
  "browser_action": {
    "default_title": "Felo Search"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{376492d1-67c4-4888-9341-e071eef1e276}"
    }
  }
}