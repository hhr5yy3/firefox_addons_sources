{
    "manifest_version": 2,

    "name": "Adobe Experience Cloud Bookmarks",
    "short_name": "Adobe Bookmarks",
    "description": "Bookmarks to Adobe Experience Cloud: Analytics, AEP Launch, Target, Campaign, AEM, DTM and other solutions, core services.",
    "version": "1.4.40.5",
    "author": "Andrey Osadchuk",

    "browser_specific_settings": {
        "gecko": {
            "id": "adobe@osadchuka.com",
            "strict_min_version": "57.0"
        }
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },

    "icons": {
        "16": "img/extension_icon16.png",
        "32": "img/extension_icon32.png",
        "48": "img/extension_icon48.png",
        "64": "img/extension_icon64.png",
        "128": "img/extension_icon128.png"
    },

    "permissions": ["storage","activeTab","https://*.status.adobe.com/"],
    "optional_permissions": ["https://*.omniture.com/", "https://*.an.adobe.com/", "https://experience.adobe.com/", "https://*.experiencecloud.adobe.com/", "https://launch.adobe.com/", "https://assets.adobedtm.com/"],

    "background": {
        "scripts": ["jquery-3.2.1.min.js","popup.js"]
    },

    "browser_action": {
        "default_icon": {
            "16": "img/extension_icon16.png",
            "32": "img/extension_icon32.png",
            "48": "img/extension_icon48.png",
            "64": "img/extension_icon64.png",
            "128": "img/extension_icon128.png"
        },
        "default_popup": "popup.html"
    },

    "web_accessible_resources": ["img/*.png"]
}