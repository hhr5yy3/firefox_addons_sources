},{
"name":"Built With",
"template":"https://builtwith.com/{domainName}"
},{
"name":"URL Parser",
"template":"javascript:{const u=new URL('{fullurl}');alert(('href,protocol,username,password,hostname,port,pathname,search,hash').split(',').filter(p=>u[p]).map(p=>(p+':  '+u[p])).join('\\n'));}"
},{
"name":"Ping",
