{
  "name": "Lecture Carte Vitale",
  "version": "1.1.1",
  "manifest_version": 2,
  "description": "Lecture des donn\u00e9es d'une carte Vitale",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "*://*.gie-sv.fr/*",
        "*://*.sesam-vitale.fr/*",
        "*://*.ramage/*",
        "*://*.ameli.fr/*",
        "*://*.dmp.fr/*",
        "*://*.gouv.fr/*",
        "*://*.cnamts.fr/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "nativeMessaging"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "webextension.vitale@giesv.fr",
      "strict_min_version": "58.0"
    }
  }
}