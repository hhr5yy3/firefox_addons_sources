    "contextMenus",
    "cookies",
    "idle",
    "privacy",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking"
  ],
  "optional_permissions": [
    "downloads"
  ],
  "version": "6.2502.0",
  "content_security_policy": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'",
