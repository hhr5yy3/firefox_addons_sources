{
  "name": "Plugin el-Dok",
  "description": "Plugin el-Dok",
  "version": "1.42",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery-3.5.1.min.js",
        "content.js"
      ],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "browser_action": {
    "default_icon": "logo_eldok_login.png"
  },
  "manifest_version": 2,
  "permissions": [
    "nativeMessaging",
    "tabs",
    "activeTab",
    "management"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{75b301b7-a89e-4aa1-8dab-26df0001ea16}"
    }
  }
}