{
  "manifest_version": 2,
  "name": "Navegador PJe - FF Extesion",
  "version": "1.0.9",
  "description": "Permite o acesso ao Navegador PJe e utilit\u00e1rios de forma facilitada atrav\u00e9s de uma extens\u00e3o do navegador.",
  "icons": {
    "128": "icone.png"
  },
  "browser_action": {
    "default_icon": "icone.png",
    "default_title": "Navegador PJe - FF Extesion",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.mozilla.org/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/pjeextscript.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{6cf83514-2264-4476-8c37-8adcd81bbb46}"
    }
  }
}