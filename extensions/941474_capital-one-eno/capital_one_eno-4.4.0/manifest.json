{
  "manifest_version": 2,
  "name": "Eno\u00ae from Capital One\u00ae",
  "description": "Shop more securely through your desktop browser with Eno\u00ae, your Capital One\u00ae assistant.",
  "short_name": "Eno\u00ae from Capital One\u00ae",
  "version": "4.4.0",
  "icons": {
    "16": "assets/images/app-icon.png",
    "48": "assets/images/app-icon48.png",
    "128": "assets/images/app-icon128.png"
  },
  "browser_action": {
    "default_icon": "assets/images/app-icon.png"
  },
  "applications": {
    "gecko": {
      "id": "{4d5b7a5e-5232-9e45-97f4-f8e1ca2626e5}",
      "strict_min_version": "57.0"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "all_frames": false,
      "css": [
        "assets/css/dnd_card.css"
      ]
    },
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "all_frames": true,
      "js": [
        "content-scripts.min.js",
        "cs.min.js",
        "content-script.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "refresh.js",
      "background.min.js",
      "bg.min.js",
      "background-script.js",
      "jsrsasign.js",
      "assets/vendor/snowplow.min.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "alarms",
    "https://*.capitalone.com/*",
    "http://*.capitalone.com/*"
  ],
  "web_accessible_resources": [
    "assets/*",
    "index.html",
    "ccswap.html",
    "ccswap*.js",
    "automaton.min.js",
    "consent/*"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}