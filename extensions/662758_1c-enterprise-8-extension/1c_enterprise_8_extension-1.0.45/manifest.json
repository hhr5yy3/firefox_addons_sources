{
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "manifest_version": 2,
  "version": "1.0.45",
  "icons": {
    "128": "logo.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "consts.js",
      "datav4.js",
      "dataactualizer.js",
      "nativeenvironment.js",
      "permission.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "consts.js",
        "nativeenvironment.js",
        "permission.js",
        "contentscript.js"
      ],
      "run_at": "document_start"
    }
  ],
  "applications": {
    "gecko": {
      "id": "1centext_ff@1c.ru",
      "strict_min_version": "52.0"
    }
  },
  "permissions": [
    "nativeMessaging",
    "clipboardWrite",
    "clipboardRead",
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "manifest.json"
  ],
  "options_ui": {
    "page": "options.html"
  }
}