{
  "name": "__MSG_appName__",
  "description": "__MSG_appDescFirefox__",
  "default_locale": "en",
  "version": "4.23.0",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16-active.png",
    "32": "icons/icon32-active.png",
    "48": "icons/icon48-active.png",
    "64": "icons/icon64-active.png",
    "128": "icons/icon128-active.png"
  },
  "background": {
    "scripts": [
      "background.bundle.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "__MSG_appName__",
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
    "Roboto-*.woff",
    "Roboto-*.woff2",
    "/web_accessible_resources/*",
    "autofill.css",
    "fonts.css",
    "autofill-action-light.svg",
    "autofill-action-dark.svg"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://surfshark.com/download/firefox/onboarding*"
      ],
      "js": [
        "cs-onboarding.bundle.js"
      ],
      "run_at": "document_start"
    },
    {
      "match_about_blank": true,
      "all_frames": true,
      "js": [
        "cleanweb.bundle.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    },
    {
      "match_about_blank": false,
      "all_frames": false,
      "js": [
        "autofill.bundle.js"
      ],
      "css": [
        "fonts.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://my.surfshark.com/account/extension-login-cs*"
      ],
      "js": [
        "cs-extension-login.bundle.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "proxy",
    "webRequest",
    "webNavigation",
    "privacy",
    "tabs",
    "contextMenus",
    "storage",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self'; img-src 'self' https://cdn.ss-cdn.com/;; object-src 'none'",
  "browser_specific_settings": {
    "gecko": {
      "id": "{732216ec-0dab-43bb-ac85-4b5e1977599d}",
      "strict_min_version": "91.1.0"
    }
  }
}