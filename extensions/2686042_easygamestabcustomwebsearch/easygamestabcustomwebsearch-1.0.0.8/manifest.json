{
  "background": {
    "scripts": [
      "distJs/constants.js",
      "distJs/helper.js",
      "html/js/storageReplacer.js",
      "util.js",
      "background.js",
      "misc.js"
    ]
  },
  "name": "Easy Games Tab & Custom Web Search",
  "short_name": "Easy Games Tab",
  "icons": {
    "128": "icons/iconc_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://easygamestab.com/*"
      ],
      "run_at": "document_start",
      "all_frames": false,
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "description": "Add the Easy Games Tab & Custom Web Search extension & click the favicon on your toolbar to get free games & Yahoo\u00ae search.",
  "version": "1.0.0.8",
  "manifest_version": 2,
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "128": "icons/icon128.png"
    },
    "default_title": "Easy Games Tab & Custom Web Search"
  },
  "permissions": [
    "*://easygamestab.com/",
    "tabs",
    "storage"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "web search by Yahoo",
      "keyword": "Easy Games Tab",
      "favicon_url": "icons/icon128.png",
      "search_url": "https://search.yahoo.com/yhs/search?hspart=mnet&hsimp=yhs-001&type=type9049412-spa-3583-84499&param1=3583&param2=84499&p={searchTerms}",
      "suggest_url": "https://search.yahoo.com/sugg/ie?output=fxjson&command={searchTerms}&nResults=10",
      "is_default": true
    }
  },
  "applications": {
    "gecko": {
      "id": "Easy_Games_Tab_zKsDuEzLrv@easygamestab.com"
    }
  }
}