{
  "manifest_version": 2,
  "name": "EE2ME",
  "description": "Import from easyenjaz",
  "version": "1.0.4.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Import from Easyenjaz"
  },
  "icons": {
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "clipboardRead",
    "clipboardWrite",
    "storage"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{67e163fd-7437-4b83-98f1-0eb569cc2cbc}"
    }
  }
}