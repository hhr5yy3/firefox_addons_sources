{
  "manifest_version": 2,
  "name": "Remove bloqueador de an\u00fancios da Abril",
  "version": "1.1.0",
  "description": "Desativa o bloqueador de AdBlocks da Veja, Exame, e os outros sites da Abril.",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "60": "icons/icon-60.png",
    "72": "icons/icon-72.png",
    "76": "icons/icon-76.png",
    "128": "icons/icon-128.png",
    "256": "icons/icon-256.png",
    "512": "icons/icon-512.png"
  },
  "permissions": [
    "webNavigation",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.abril.com.br/*"
      ],
      "js": [
        "unblock.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{1856dc44-2e34-4e6f-93fc-e4bec0c54437}"
    }
  }
}