{
    "manifest_version": 2,
    "name": "__MSG_appname__",
    "version": "1.4",
    "description": "__MSG_appdesc__",
    "default_locale": "ru",
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "img/icon16gray.png"
        },
        "default_title": "CloudVPN", 
	"default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>",
        "storage", 
	"proxy",
	"webRequest",
	"webRequestBlocking"
    ],
    "background": {
        "scripts": ["js/background.js"]
    }
}