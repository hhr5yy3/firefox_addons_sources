{
  "name": "SignNet SoftNet Sp. Z O.O.",
  "version": "1.0.8",
  "manifest_version": 2,
  "description": "Aplikacja do wykorzystywania podpisu kwalifikowanego w przegl\u0105darce.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "nativeMessaging"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{d2cf6fd6-a43f-47ba-ab83-da601e69294c}",
      "strict_min_version": "42.0"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/eCorpoNet/*"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "content.js"
      ]
    }
  ]
}