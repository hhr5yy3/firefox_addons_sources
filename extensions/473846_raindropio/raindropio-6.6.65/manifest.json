{
  "manifest_version": 3,
  "version": "6.6.65",
  "name": "Raindrop.io",
  "description": "__MSG_appDesc__",
  "homepage_url": "https://app.raindrop.io",
  "author": "Mussabekov Rustem",
  "short_name": "Raindrop.io",
  "default_locale": "en",
  "browser_specific_settings": {
    "gecko": {
      "id": "jid0-adyhmvsP91nUO8pRv0Mn2VKeB84@jetpack"
    }
  },
  "icons": {
    "16": "assets/icon_16.png",
    "32": "assets/icon_32.png",
    "48": "assets/icon_48.png",
    "128": "assets/icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "action": {
    "default_popup": "index.html?action",
    "default_icon": {},
    "theme_icons": [
      {
        "light": "assets/action_firefox_dark_16.png",
        "dark": "assets/action_firefox_light_16.png",
        "size": 16
      },
      {
        "light": "assets/action_firefox_dark_24.png",
        "dark": "assets/action_firefox_light_24.png",
        "size": 24
      },
      {
        "light": "assets/action_firefox_dark_32.png",
        "dark": "assets/action_firefox_light_32.png",
        "size": 32
      }
    ]
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "storage"
  ],
  "optional_permissions": [
    "tabs"
  ],
  "host_permissions": [
    "*://api.raindrop.io/*"
  ],
  "optional_host_permissions": [
    "*://*/*"
  ],
  "omnibox": {
    "keyword": "rd"
  },
  "commands": {
    "_execute_action": {},
    "save_page": {
      "suggested_key": {
        "default": "Alt+X",
        "mac": "MacCtrl+X"
      },
      "description": "__MSG_savePageOrHighlight__"
    },
    "open_raindrop_web": {
      "description": "__MSG_openRaindrop__"
    },
    "_execute_sidebar_action": {
      "suggested_key": {
        "default": "Ctrl+Period"
      },
      "description": "__MSG_openSidePanel__"
    }
  },
  "sidebar_action": {
    "default_panel": "sidepanel.html",
    "default_icon": {
      "16": "assets/action_firefox_light_16.png",
      "24": "assets/action_firefox_light_24.png",
      "32": "assets/action_firefox_light_32.png"
    },
    "browser_style": false,
    "open_at_install": false
  }
}