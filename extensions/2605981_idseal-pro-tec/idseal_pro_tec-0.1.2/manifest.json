{
	"desrciption": "IDSeal Pro-Tec scans, finds and blocks online tracking and privacy invasion attempts.",
	"manifest_version": 2,
	"name": "IDSeal Pro-Tec",
    "homepage_url": "https://idseal.com",
	"author": "IDSeal LLC",
	"version": "0.1.2",
	"icons": {
		"128": "Resources/icon128.png",
		"48": "Resources/icon48.png"
	},
	"permissions": [
		"privacy",
		"cookies",
		"storage",
		"tabs",
		"webNavigation",
		"webRequest",
		"webRequestBlocking",
		"http://*/*",
		"https://*/*"
	],
	"browser_action": {
		"default_icon": "Resources/icon48.png",
		"default_title": "IDSeal Pro-Tec",
		"default_popup": "popup.html"
	},
	"web_accessible_resources": [
		"src/common.js",
		"src/inject.js",
		"resources/icon16.png",
		"resources/icon48.png",
		"resources/icon128.png",
		"resources/ext_popup_txtboxfield.png",
		"popup.html"
    ],
	"background": {
		"scripts": [
			"src/filters.js",
			"src/adblocker.js",
			"src/md5.js",
			"src/tabs.js",
			"src/common.js",
            "src/websocket2.js",
			"src/vars.js",
            "src/language.js",
			"src/background.js"
		], 
		"persistent": true
	},
		"applications": {
        "gecko": {
            "id": "idseal_pro_tec@idseal.com", 
		    "strict_min_version": "52.0"
        }
	}
}