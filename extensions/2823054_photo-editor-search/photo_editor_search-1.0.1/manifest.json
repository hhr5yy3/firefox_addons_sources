{
  "name": "Photo Editor & Search",
  "description": "Photo Editor & Search",
  "version": "1.0.1",
  "manifest_version": 2,
  "browser_action": {
    "browser_style": true,
    "default_title": "Photo Editor & Search"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://photoeditor.net/*"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Yahoo by Photo Editor & Search",
      "keyword": "Photo Editor & Search",
      "search_url": "https://search.yahoo.com/yhs/search?hspart=ata&hsimp=yhs-002&type=type9067382-aal-190001-190002&param1=190001&param2=190002&p={searchTerms}&grd=1",
      "favicon_url": "https://www.yahoo.com/favicon.ico",
      "suggest_url": "https://search.yahoo.com/sugg/ie?output=fxjson&command={searchTerms}&nResults=10",
      "is_default": true
    }
  },
  "background": {
    "scripts": [
      "./extensionScripts/background.js"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "addon@photoeditor.net",
      "strict_min_version": "98.0"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://photoeditor.net/*"
      ],
      "run_at": "document_end",
      "all_frames": false,
      "js": [
        "./extensionScripts/contentscript.js"
      ]
    }
  ],
  "icons": {
    "48": "./assets/images/extIcon.png"
  }
}