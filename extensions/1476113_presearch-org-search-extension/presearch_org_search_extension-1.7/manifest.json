{
  "manifest_version": 2,
  "name": "Presearch.com Search Extension",
  "description": "Set your browser's default search to use Presearch.com",
  "version": "1.7",
  "homepage_url": "https://www.presearch.com/?utm_source=searchext",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon16.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.presearch.com/*"
      ],
      "js": [
        "./assets/send-message.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Presearch",
      "keyword": "Presearch.com",
      "search_url": "https://presearch.com/extsearch?term={searchTerms}",
      "favicon_url": "https://www.presearch.com/favicon.ico",
      "suggest_url": "https://presearch.com/api/suggest?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{441ae0e7-c955-4b0e-8209-99462af458db}"
    }
  }
}