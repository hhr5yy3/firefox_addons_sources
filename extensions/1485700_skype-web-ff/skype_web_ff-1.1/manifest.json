{
    "manifest_version": 2,
    "name": "Skype Web FF",
    "homepage_url": "https://gitlab.com/Newbyte/skype-web-ff",
    "version": "1.1",

    "description": "Makes using Skype Web in Firefox a breeze",

    "permissions": [
        "webRequest", "webRequestBlocking", "https://web.skype.com/*", "https://preview.web.skype.com/*", "https://join.skype.com/*"
    ],

    "background": {
        "scripts": [ "background.js" ]
    }
}