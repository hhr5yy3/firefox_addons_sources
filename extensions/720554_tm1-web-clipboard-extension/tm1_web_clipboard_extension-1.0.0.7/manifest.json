{
  "manifest_version": 2,
  "name": "IBM Cognos TM1 Web Clipboard Extension",
  "description": "This extension allows IBM Cognos TM1 Web access to the clipboard.",
  "version": "1.0.0.7",
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["contentscript.js"],
    "all_frames": true
  }],
  "permissions": [
    "clipboardRead",
    "clipboardWrite"
  ],
  "icons": {
    "16": "icon_16x16.png",
    "48": "icon_48x48.png",
    "128": "icon_128x128.png"
  }
}
