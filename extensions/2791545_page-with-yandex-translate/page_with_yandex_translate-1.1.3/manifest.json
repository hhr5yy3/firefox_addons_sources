{
  "description": "Open current page with Yandex.Translator",
  "manifest_version": 2,
  "name": "Yandex.Translate",
  "version": "1.1.3",
  "homepage_url": "https://github.com/IriyAleo/firefox-extension-yandex-translate",
  "icons": {
    "48": "icons/Yandex.Translate.png"
  },
  "browser_action": {
    "default_icon": "icons/Yandex.Translate.png",
    "default_title": "Yandex.Translate",
    "default_popup": "popup/choose_translate.html"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{cd7a5a3a-eb0a-4eba-aaeb-476843143eeb}"
    }
  }
}