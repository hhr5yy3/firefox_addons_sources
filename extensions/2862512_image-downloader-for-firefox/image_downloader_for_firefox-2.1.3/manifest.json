{
  "manifest_version": 3,
  "name": "__MSG_appName__",
  "version": "2.1.3",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start",
      "js": [
        "contentScript.js"
      ],
      "all_frames": false
    }
  ],
  "action": {
    "default_icon": {
      "16": "assets/icon16.png",
      "32": "assets/icon32.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    },
    "default_popup": "popup.html",
    "default_title": "__MSG_appName__"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "storage",
    "downloads",
    "sidePanel"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{da8a8994-07be-4b0e-91dd-dd99863fd2c5}",
      "strict_min_version": "128.0"
    }
  },
  "host_permissions": [
    "<all_urls>"
  ]
}