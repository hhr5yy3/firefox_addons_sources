{
  "manifest_version": 2,
  "key": "",
  "name": "Saby Plugin Extension",
  "version": "23.3200.17",
  "description": "\u0420\u0430\u0441\u0448\u0438\u0440\u0435\u043d\u0438\u0435 \u0431\u0440\u0430\u0443\u0437\u0435\u0440\u0430 \u0434\u043b\u044f \u0440\u0430\u0431\u043e\u0442\u044b \u0441\u043e \u0421\u0411\u0418\u0421 \u041f\u043b\u0430\u0433\u0438\u043d.",
  "permissions": [
    "storage",
    "tabs",
    "nativeMessaging",
    "alarms",
    "scripting",
    "downloads",
    "notifications",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "applications": {
