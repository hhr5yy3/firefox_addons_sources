{
  "manifest_version": 3,
  "short_name": "NordVPN",
  "version": "4.9.0",
  "host_permissions": [
    "<all_urls>"
  ],
  "incognito": "spanning",
  "icons": {
    "16": "./icons/nordvpn-16-active.png",
    "32": "./icons/nordvpn-32-active.png",
    "48": "./icons/nordvpn-48-active.png",
    "128": "./icons/nordvpn-128-active.png"
  },
  "action": {
    "default_icon": "icons/nordvpn-48-inactive.png",
    "default_popup": "index.html",
    "default_title": "NordVPN"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "csNotification.bundle.css",
        "csKillSwitch.bundle.css",
        "killswitch.html",
        "fingerprint.html"
      ]
    }
  ],
  "name": "NordVPN - a VPN proxy extension for Firefox",
  "description": "One click, and you're connected to the fastest VPN out there \u2014 NordVPN. Stay safe online with our VPN proxy extension for Firefox.",
  "browser_specific_settings": {
    "gecko": {
      "id": "nordvpnproxy@nordvpn.com",
      "strict_min_version": "113.0"
    }
  },
  "background": {
    "scripts": [
      "background.bundle.js"
    ]
  },
  "permissions": [
    "alarms",
    "scripting",
    "proxy",
    "webRequest",
    "webNavigation",
    "privacy",
    "storage",
    "notifications",
    "tabs",
    "contextMenus",
    "activeTab",
    "unlimitedStorage",
    "downloads",
    "webRequestBlocking",
    "dns"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}