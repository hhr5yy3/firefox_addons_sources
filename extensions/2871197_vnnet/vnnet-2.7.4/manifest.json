{
  "name": "VNNet",
  "permissions": [
    "storage",
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "contextMenus"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "./popup/background.js"
    ]
  },
  "action": {
    "default_popup": "popup/index.html"
  },
  "description": "Free VPN addon",
  "icons": {
    "512": "icon.png"
  },
  "default_locale": "en",
  "manifest_version": 3,
  "version": "2.7.4",
  "browser_specific_settings": {
    "gecko": {
      "id": "poteto@vnnet",
      "strict_min_version": "91.1.0"
    }
  }
}