{
   "author": "Providec",
   "name": "__MSG_ext_name_yandex__",
   "description": "__MSG_ext_description_yandex__",
   "default_locale": "en",
   "version": "3.2.19",
   "manifest_version": 2,
   "background": {
      "page": "background.html",
      "persistent": false
   },
   "browser_action": {
      "default_icon": {
         "16": "icons/icon_16.png"
      },
      "default_title": "__MSG_ext_default_title_yandex__"
   },
   "options_ui": {
    "page": "options.html",
    "open_in_tab": true
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "inc/global.functions.js", "inc/storage.local.js", "onlinezakladki.js" ],
      "matches": [ "https://onlinezakladki.ru/*", "https://bk.tools/*" ],
      "run_at": "document_start"
   } ],
   "icons": {
      "128": "icons/icon_128.png",
      "16": "icons/icon_16.png",
      "48": "icons/icon_48.png"
   },
   "incognito": "split",
   "permissions": [ 
      "bookmarks", 
	  "notifications", 
	  "contextMenus", 
	  "tabs", 
	  "activeTab", 
	  "storage", 
	  "unlimitedStorage", 
	  "<all_urls>" 
	    ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "key": ""
}