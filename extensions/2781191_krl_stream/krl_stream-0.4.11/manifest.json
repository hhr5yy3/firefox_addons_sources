{
  "author": "tiiBz",
  "description": "L'extension qui t'informe si KRL est en LIVE sur Twitch !",
  "name": "KRL_STREAM",
  "version": "0.4.11",
  "icons": {
    "16": "icons/krl16.png",
    "32": "icons/krl32.png",
    "48": "icons/krl48.png",
    "128": "icons/krl128.png"
  },
  "permissions": [
    "storage",
    "alarms",
    "notifications",
    "*://*.krl.tiibz.dev/*",
    "*://*.ytimg.com/*",
    "*://*.hltv.org/*",
    "*://static-cdn.jtvnw.net/*"
  ],
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/krl16.png",
      "32": "icons/krl32.png",
      "48": "icons/krl48.png",
      "128": "icons/krl128.png"
    },
    "default_popup": "src/entries/popup/index.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "krl_stream-extension@tiiBz"
    }
  },
  "manifest_version": 2
}