{
  "manifest_version": 2,
  "name": "7 Times Faster",
  "short_name": "7",
  "version": "1.0.2.2resigned1",
  "author": "Maxym Makarchuk",
  "description": "It's a fastest ad blocker. Its speed up your internet, sometimes in two times and more. By blocking ads, malware and tracking",
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "64": "icons/64x64.png",
    "128": "icons/128x128.png"
  },
  "background": {
    "scripts": [
      "background.js",
      "blockedlist.js",
      "blockedlisttop100.js",
      "blockedlisttop200.js",
      "blockedlisttop300.js",
      "blockedlistparturl.js",
      "blockedlisthostname.js",
      "blockSocialDomain.js",
      "blockSocialHost.js",
      "blockSocialURL.js",
      "socialMediaDomain.js",
      "socialMediaHost.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "<all_urls>",
    "webNavigation",
    "storage"
  ],
  "browser_action": {
    "default_title": "7 Times Faster",
    "default_icon": "icons/48x48.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "donation.html"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{166fb5a6-847f-4def-94fd-a66b5c246b3e}"
    }
  }
}