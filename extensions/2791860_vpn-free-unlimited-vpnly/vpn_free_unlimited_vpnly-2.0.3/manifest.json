{
  "name": "__MSG_appName__",
  "version": "2.0.3",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png",
    "128": "icons/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "background.html"
