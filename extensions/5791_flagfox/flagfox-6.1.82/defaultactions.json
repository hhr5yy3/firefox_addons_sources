"show":true
},{
"name":"Traceroute",
"template":"formfield:https://traceroute-online.com/|target|{domainName}"
},{
"name":"Netcraft",
"template":"https://toolbar.netcraft.com/site_report?url={domainName}"
},{
"name":"IntoDNS",
