{
  "manifest_version": 2,
  "name": "Quizzard",
  "description": "Quick and easy tool to rapidly search for homework questions, definitions and quiz answers.",
  "version": "4.3.1",
  "homepage_url": "https://www.quizzardapp.com/home",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Quizzard",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "96": "icons/icon_96.png",
    "128": "icons/icon_128.png",
    "256": "icons/icon_256.png"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "<all_urls>",
    "https://www.quizzardapp.com/*",
    "https://www.google.com/*",
    "https://quizlet.com/*"
  ],
  "background": {
    "scripts": [
      "./static/js/background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "./static/js/interval.js"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{ebc5d633-6c9c-4b22-8565-2a0b6db7ca1c}"
    }
  }
}