{
  "manifest_version": 2,
  "name": "Fusion Tables",
  "description": "Easy to Search and Find Tables of the Oracle Fusion Suite",
  "author": "Marinache Radu Viorel",
  "version": "2.3",
  "icons": {
    "16": "icons/icon16.png",
    "24": "icons/icon24.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "512": "icons/icon512.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "contextMenus",
    "webRequest",
    "clipboardWrite",
    "storage",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/icon64.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "keep.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{ef8f53f9-c47b-47bc-a5e9-9a9e80ec3161}"
    }
  }
}