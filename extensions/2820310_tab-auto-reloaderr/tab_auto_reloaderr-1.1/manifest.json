{
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_popup": "popup.html",
    "default_title": "Reload this page every\u2026"
  },
  "description": "This extension reloads the current tab at a chosen interval.",
  "homepage_url": "https://github.com/denilsonsa/crx-reload-tab/",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Tab auto reloader",
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "short_name": "Tab reloader",
  "version": "1.1",
  "browser_specific_settings": {
    "gecko": {
      "id": "{e3d28294-07ee-4074-8e06-323f8a7a2b8d}"
    }
  }
}