{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "ContentScript.js"
      ],
      "match_about_blank": true,
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "applications": {
    "gecko": {
      "id": "security@eagleyun.com",
      "strict_min_version": "68.0"
    }
  },
  "description": "Transbay Extension",
  "icons": {
    "16": "icons/Transbay.png",
    "48": "icons/Transbay.png",
    "128": "icons/Transbay.png"
  },
  "manifest_version": 2,
  "name": "Transbay-Extension",
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "version": "1.8"
}