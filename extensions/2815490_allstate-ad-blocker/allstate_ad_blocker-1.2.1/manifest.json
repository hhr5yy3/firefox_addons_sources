{
  "manifest_version": 2,
  "version": "1.2.1",
  "default_locale": "en_US",
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "48": "/images/man-icon-48.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "tabs",
    "webNavigation",
    "https://srs-s3-aip-prod-static0r0a0bucketebb21caf-1j71mf77cgr4q.s3.amazonaws.com/*",
    "https://atb-s3-aip-prod-static0r0a0bucketebb21caf-1lmozxdboi0zx.s3.amazonaws.com/*",
    "https://*/*",
    "http://*/*",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "images/*.png",
    "*.png",
    "fonts/*",
    "available-locales.json"
  ],
  "browser_action": {
    "default_icon": {
      "48": "/images/status-disabled-48.png"
    },
    "default_title": "__MSG_extensionToolTip__",
    "default_popup": "html/popup.html"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "strip-tracking-parameters",
        "enabled": false,
        "path": "rules/strip-tracking-parameters.json"
      }
    ]
  },
  "incognito": "not_allowed",
  "content_scripts": [
    {
      "matches": [
        "https://allstate.sudoplatform.com/redirect_uri.html",
        "https://allstate.sudoplatform.com/redirect_uri.html?*"
      ],
      "js": [
        "content/cognito-redirect-handler.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'wasm-eval'; object-src 'self';",
  "browser_specific_settings": {
    "gecko": {
      "id": "{4265b7e2-0231-4b78-bb8e-6ed2b3793c4a}"
    }
  }
}