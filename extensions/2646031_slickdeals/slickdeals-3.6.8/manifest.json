{
  "browser_action": {
    "default_popup": "src/popup/index.html"
  },
  "author": "Slickdeals",
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "content_scripts": [
    {
      "js": [
        "src/entries/contentScript/globalStart/index.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": true,
      "js": [
        "src/contentScript/slickdealsDealDetails/index.js"
      ],
      "matches": [
        "https://slickdeals.net/f/*",
        "https://slickdeals.net/e/*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "src/contentScript/slickdealsIdle/index.js"
      ],
      "matches": [
        "https://*.slickdeals.net/*"
      ],
      "run_at": "document_idle"
    },
    {
      "all_frames": true,
      "js": [
        "src/contentScript/slickdealsStart/index.js"
      ],
      "matches": [
        "https://*.slickdeals.net/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Always get the best price: find the best deals from Slickdeals and automatically apply coupons!",
  "homepage_url": "https://slickdeals.net/",
  "icons": {
    "16": "icons/16.png",
    "19": "icons/19.png",
    "32": "icons/32.png",
    "38": "icons/38.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "96": "icons/96.png",
    "128": "icons/128.png",
    "256": "icons/256.png",
    "512": "icons/512.png"
  },
  "incognito": "not_allowed",
  "manifest_version": 2,
  "minimum_chrome_version": "102",
  "name": "Slickdeals: Automatic Coupons and Deals",
  "permissions": [
    "alarms",
    "cookies",
    "notifications",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "*://*/*"
  ],
  "version": "3.6.8",
  "browser_specific_settings": {
    "gecko": {
      "id": "firefox.extension@slickdeals.net",
      "strict_min_version": "117.0"
