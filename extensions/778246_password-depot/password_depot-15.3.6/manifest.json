{
  "name": "Password Depot Extension",
  "version": "15.3.6",
  "manifest_version": 2,
  "description": "__MSG_pd_description__",
  "homepage_url": "https://www.password-depot.de",
  "default_locale": "en",
  "applications": {
    "gecko": {
      "id": "{427859bd-061c-4095-bd2b-845962f45f43}"
    }
  },
  "icons": {
    "128": "icons/128.png",
    "48": "icons/48.png",
    "16": "icons/16.png"
  },
  "background": {
    "scripts": [
      "vendor/jquery-3.5.1.min.js",
      "js/pdBackgroundScript.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "vendor/underscore-min.js",
        "vendor/jquery-3.5.1.min.js",
        "js/pdContentScript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": "icons/48.png",
    "default_title": "Password Depot",
    "default_popup": "dialogs/popup.html"
  },
  "permissions": [
    "nativeMessaging",
    "storage"
  ],
  "web_accessible_resources": [
    "dialogs/*",
    "icons/*",
    "fonts/*",
    "assets/*"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}