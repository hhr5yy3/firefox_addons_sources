{
  "manifest_version": 2,
  "version": "26.0",
  "author": "Juraj M\u00e4siar",
  "default_locale": "en",
  "name": "__MSG_app_name__",
  "description": "__MSG_app_desc__",
  "icons": {
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "background_gsd.worker.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_area": "navbar",
    "default_icon": {
      "128": "128.png"
    },
    "default_popup": "popup/popup.html?popup=true"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+O"
      }
    },
    "open-dials": {
      "suggested_key": {
        "default": "F4"
      },
      "description": "Opens dials page"
    },
    "search-dial": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      },
      "description": "Opens search for dial box"
    },
    "edit-group": {
      "suggested_key": {
        "default": "Alt+R"
      },
      "description": "Opens \"Edit group\" dialog"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://group-speed-dial.fastaddons.com/*"
      ],
      "run_at": "document_start",
      "js": [
        "content_scripts/content_script.js"
      ]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "dial.html"
  },
  "chrome_settings_overrides": {
    "homepage": "dial.html"
  },
  "options_ui": {
    "page": "popup/popup.html",
    "open_in_tab": true
  },
  "permissions": [
    "alarms",
    "webNavigation",
    "contextMenus",
    "storage",
    "tabs",
    "activeTab",
    "webRequest",
    "declarativeNetRequestWithHostAccess",
    "scripting",
    "<all_urls>",
    "contextualIdentities",
    "cookies",
    "tabHide"
  ],
  "optional_permissions": [
    "downloads",
    "sessions",
    "bookmarks",
    "topSites"
  ],
  "web_accessible_resources": [
    "128.png"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "admin@fastaddons.com_GroupSpeedDial",
      "strict_min_version": "115.0"
    },
    "gecko_android": {
      "strict_min_version": "121.0"
    }
  }
}