{
  "name": "RetailMeNot: Codes, Cash Back and Coupons",
  "description": "Automatically finds the best promo codes and cash back as you shop online.",
  "version": "4.5.6",
  "manifest_version": 2,
  "browser_specific_settings": {
    "gecko": {
      "id": "retailmenot-genie@rmn.com",
      "strict_min_version": "115.0"
    }
  },
  "background": {
    "scripts": [
      "background.bundle.js"
    ],
    "persistent": true
  },
  "browser_action": {},
  "permissions": [
    "alarms",
    "<all_urls>",
    "cookies",
    "scripting",
    "storage",
    "tabs",
    "webNavigation",
    "webRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "icons": {
    "16": "./html/images/icon_16.png",
    "32": "./html/images/icon_32.png",
    "64": "./html/images/icon_64.png",
    "128": "./html/images/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "inject.bundle.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "./html/consent.html"
  ]
}