[{
"name":"Geotool",
"template":"https://iplookup.flagfox.net/?ip={IPaddress}&host={domainName}",
"iconclick":"click",
"hotkey":"Alt+Shift+G",
"show":true
},{
"name":"WolframAlpha",
"template":"https://www.wolframalpha.com/input/?i={domainName}"
},{
"name":"Whois",
"template":"https://whois.domaintools.com/{domainName}",
"iconclick":"middleclick",
"show":true
},{
"name":"Virus Scan",
"template":"https://www.virustotal.com/#/domain/{domainName}",
"show":true
},{
"name":"Check Server Status",
"template":"https://isitup.org/{domainName}",
"iconclick":"doubleclick",
"show":true
},{
"name":"Google",
"template":"https://www.google.com/search?q=site:{domainName}"
},{
"name":"Google Translate",
"template":"https://translate.google.com/translate?tl={locale}&u={fullUrl}",
"show":true
},{
"name":"Google Cache",
"template":"https://webcache.googleusercontent.com/search?q=cache:{fullURL}",
