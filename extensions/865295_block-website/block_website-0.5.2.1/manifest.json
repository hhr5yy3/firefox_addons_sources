{
  "name": "Block Site",
  "version": "0.5.2.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "/v2/chrome.runtime.js",
      "/v2/chrome.windows.create.js",
      "/v2/chrome.windows.get.js",
      "/v2/chrome.windows.getCurrent.js",
      "/v2/chrome.action.js",
      "/v2/chrome.tabs.query.js",
      "/v2/chrome.alarms.get.js",
      "/v2/chrome.alarms.getAll.js",
      "/v2/chrome.contextMenus.create.js",
      "helper.js",
      "blocker.js",
      "schedule.js",
      "contextmenu.js",
      "worker.js"
    ]
  },
  "description": "__MSG_app_description__",
  "default_locale": "en",
  "permissions": [
    "activeTab",
    "storage",
    "notifications",
    "alarms",
    "contextMenus",
    "declarativeNetRequestWithHostAccess",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "storage": {
    "managed_schema": "schema.json"
  },
  "icons": {
    "16": "/data/icons/16.png",
    "32": "/data/icons/32.png",
    "48": "/data/icons/48.png",
    "64": "/data/icons/64.png",
    "128": "/data/icons/128.png",
    "256": "/data/icons/256.png"
  },
  "homepage_url": "https://webextension.org/listing/block-site.html",
  "options_ui": {
    "page": "/data/options/index.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/data/inject/page-blocker.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "/data/blocked/index.html",
    "/data/close/index.html"
  ],
  "commands": {
    "_execute_browser_action": {}
  },
  "browser_action": {},
  "browser_specific_settings": {
    "gecko": {
      "id": "{54e2eb33-18eb-46ad-a4e4-1329c29f6e17}"
    }
  }
}