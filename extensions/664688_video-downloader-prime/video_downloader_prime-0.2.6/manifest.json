{
  "version": "0.2.6",
  "manifest_version": 2,
  "short_name": "download-helper",
  "name": "Video Downloader Prime",
  "background": {
    "page": "lib/chrome/background.html"
  },
  "description": "Easily download most popular video formats.",
  "homepage_url": "https://mybrowseraddon.com/download-helper.html",
  "options_ui": {
    "open_in_tab": true,
    "page": "data/options/options.html"
  },
  "permissions": [
    "storage",
    "*://*/*",
    "downloads",
    "webRequest"
  ],
  "browser_action": {
    "default_popup": "data/popup/popup.html",
    "default_title": "Video Downloader Prime",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "jid1-i6dUGvCrz2WZu8@jetpack"
    }
  }
}