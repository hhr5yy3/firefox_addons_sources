{
  "manifest_version": 2,
  "name": "Menores Pre\u00e7os Dudu Rocha",
  "description": "Encontre as melhores ofertas e cupons de descontos.",
  "version": "11.78.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "run_at": "document_end",
      "js": [
        "index.js"
      ],
      "css": [
        "index.css"
      ]
    }
  ],
  "permissions": [
    "identity"
  ],
  "icons": {
    "16": "/images/icon_16.png",
    "48": "/images/icon_48.png",
    "128": "/images/icon_128.png"
  },
  "web_accessible_resources": [
    "images/*",
    "3rdparty/*",
    "injection/*",
    "index.css"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{2c69e4a1-a0e9-424d-81db-4ed92c0bb0ec}"
    }
  }
}