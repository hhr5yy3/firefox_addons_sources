{
  "manifest_version": 2,
  "name": "YouTube 4K Downloader",
  "version": "6.5.0",
  "description": "Download YouTube Videos at (8K, 4K, 1080p, 720p) with 60 FPS and HDR in 1 click and download MP3 at the best quality.",
  "homepage_url": "https://youtube4kdownloader.com/",
  "icons": {
    "16": "assets/icons/i-16.png",
    "32": "assets/icons/i-32.png",
    "48": "assets/icons/i-48.png",
    "64": "assets/icons/i-64.png",
    "96": "assets/icons/i-96.png",
    "128": "assets/icons/i-128.png",
    "256": "assets/icons/i-256.png",
    "512": "assets/icons/i-512.png"
  },
  "background": {
    "scripts": [
      "background/bg.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://*.youtube.com/*",
    "https://*.youtube4kdownloader.com/*",
    "https://*.googlevideo.com/videoplayback*",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": [
    "assets/*.png"
  ],
  "content_scripts": [
    {
      "js": [
        "content_scripts/cs.js"
      ],
      "css": [
        "assets/ui.css"
      ],
      "matches": [
        "https://*.youtube.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "assets/icons/i-16.png",
      "32": "assets/icons/i-32.png",
      "48": "assets/icons/i-48.png",
      "64": "assets/icons/i-64.png",
      "96": "assets/icons/i-96.png",
      "128": "assets/icons/i-128.png",
      "256": "assets/icons/i-256.png",
      "512": "assets/icons/i-512.png"
    },
    "default_title": "YouTube 4K Downloader",
    "browser_style": true
  },
  "applications": {
    "gecko": {
      "id": "{58fcf736-562f-4bb3-8a60-d0dbca99ceba}"
    }
  }
}