{
  "name": "Assinatura Digital no Navegador",
  "version": "2.0.2",
  "description": "Realize Assinaturas Digitais no navegador.",
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "extension@bry.com.br"
    }
  },
  "browser_action": {
    "default_icon": {
      "19": "icon-361-grayscale.png",
      "38": "icon-361-grayscale.png"
    },
    "default_title": "Assinatura Digital no Navegador",
    "default_popup": "main.html"
  },
  "background": {
    "scripts": [
      "background.bundle.js"
    ]
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "nativeMessaging",
    "tabs",
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "css": [
        "sweetalert2.min.css",
        "bry.css"
      ],
      "js": [
        "react.bundle.js",
        "sweetalert2.bundle.js",
        "content_script.bundle.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "firefox-downloaded-file.png",
    "chrome-downloaded-file.png",
    "first-twosteps.png",
    "second-twosteps.png",
    "icon-361-grayscale.png",
    "sweetalert.css",
    "trash.png",
    "download-arrow.png",
    "ok.png",
    "loading.gif",
    "brand.png",
    "icon-128.png"
  ]
}