  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "applications": {
    "gecko": {
      "id": "{1935b298-ac62-11ec-b909-0242ac120003}",
      "strict_min_version": "91.1.0"
