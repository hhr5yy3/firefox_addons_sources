{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/128.png",
    "default_popup": "popup.html",
    "default_title": "Translator"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "css/content.css"
      ],
      "exclude_matches": [
        "*://translate.google.com/*"
      ],
      "js": [
        "js/purify.min.js",
        "js/jquery.js",
        "js/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "default_locale": "en",
  "description": "__MSG_description__",
  "icons": {
    "128": "images/128.png",
    "48": "images/48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_name__",
  "permissions": [
    "contextMenus",
    "<all_urls>",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "short_name": "Translator",
  "version": "3.0.3",
  "web_accessible_resources": [
    "js/*",
    "images/*",
    "css/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{0dc8db0b-9e11-40dd-bcb8-209a8b7ac42f}"
    }
  }
}