{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "2.0.3",
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "80.0",
      "id": "{20fc2e06-e3e4-4b2b-812b-ab431220cada}"
    }
  },
  "icons": {
    "16": "icons/sp_icon_16.png",
    "48": "icons/sp_icon_48.png",
    "128": "icons/sp_icon_128.png"
  },
  "permissions": [
    "search",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "https://*.startpage.com/*"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "__MSG_searchEngineName__",
      "keyword": "@startpage",
      "search_url": "https://www.startpage.com/do/dsearch?q={searchTerms}&cat=web&language=__MSG_extensionUrlLanguage__",
      "suggest_url": "https://www.startpage.com/suggestions?q={searchTerms}&format=opensearch&segment=startpage.defaultffx&lui=__MSG_extensionUrlLanguage__",
      "favicon_url": "https://www.startpage.com/favicon.ico",
      "is_default": true
    }
  },
  "browser_action": {
    "default_area": "navbar",
    "default_icon": {
      "16": "icons/sp_icon_16.png",
      "48": "icons/sp_icon_48.png",
      "128": "icons/sp_icon_128.png"
    },
    "default_title": "__MSG_extensionName__"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.startpage.com/*"
      ],
      "js": [
        "js/content.js"
      ],
      "css": [
        "css/content.css"
      ],
      "run_at": "document_end"
    }
  ],
  "default_locale": "en"
}