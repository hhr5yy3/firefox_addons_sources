{
  "manifest_version": 2,
  "name": "Aruba SDK Web",
  "description": "Aruba SDK Web",
  "version": "1.0",
  "icons": {
    "48": "images/icon-48.png",
    "96": "images/icon-96.png",
    "128": "images/icon-128.png",
    "256": "images/icon-256.png",
    "512": "images/icon-512.png"
  },
  "background": {
    "scripts": [
      "service_worker.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{193bc7ad-6d9e-4e70-ae0a-05ac89935b8f}"
    }
  }
}