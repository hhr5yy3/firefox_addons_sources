{
  "manifest_version": 2,
  "name": "JoyReactor Visitor Extended",
  "short_name": "JoyReactor Visitor Extended",
  "description": "",
  "author": "ExtraDJ+BloodyBody",
  "version": "1.3.7",
  "icons": {
    "128": "data/images/logo.png"
  },
  "browser_action": {
    "default_icon": "data/images/enabled.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "history",
    "downloads",
    "webRequest",
    "webRequestBlocking",
    "*://joyreactor.cc/*",
    "*://*.joyreactor.cc/*",
    "*://reactor.cc/*",
    "*://*.reactor.cc/*",
    "*://jr-proxy.com/*",
    "*://jrproxy.com/*",
    "*://joyreactor.com/*"
  ],
  "background": {
    "scripts": [
      "data/js/script.js"
    ]
  },
  "options_ui": {
    "page": "data/options.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://joyreactor.cc/*",
        "*://*.joyreactor.cc/*",
        "*://reactor.cc/*",
        "*://*.reactor.cc/*",
        "*://jr-proxy.com/*",
        "*://jrproxy.com/*",
        "*://joyreactor.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "data/js/jquery.js",
        "data/js/content.js"
      ],
      "css": [
        "data/css/content.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "data/templates.html"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{77905ede-3d90-47dc-910b-1be7e3a98a44}"
    }
  }
}