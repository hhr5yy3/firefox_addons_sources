{
  "manifest_version": 2,
  "name": "Le Troc fut\u00e9",
  "description": "Une extension pour les utilisateurs de Vinted, L'extension Troc Fut\u00e9 simplifie la re-publication, la cr\u00e9ation d'annonces et la recherche d'annonces r\u00e9centes en temps r\u00e9el sur Vinted, directement depuis notre site web Troc Fut\u00e9.",
  "version": "1.3.8.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "popup/popup.js",
        "content.js"
      ]
    }
  ],
  "background": {
    "page": "background/index.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "cookies",
    "activeTab",
    "scripting",
    "webRequest",
    "webNavigation",
    "<all_urls>",
    "webRequestBlocking",
    "webRequestFilterResponse",
    "http://*/",
    "https://*/",
    "https://developer.mozilla.org/"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self';",
  "browser_action": {
    "default_icon": "icon-square.png",
    "default_title": "Le troc fut\u00e9",
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": [
    "content.js"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{82a95230-49e4-4976-af24-567f496ce2f0}"
    }
  }
}