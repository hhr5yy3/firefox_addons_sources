{
  "manifest_version": 2,
  "name": "Gantt chart for Trello",
  "version": "1.2.4",

  "description": "The Ultimate Gantt chart add-on for Trello, brought to you by Ganttify!",

  "icons": {
    "48": "images/toolbar-icon-48.png",
    "96": "images/toolbar-icon-96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://trello.com/*"],
      "js": ["js/index.min.js"],
      "css": ["css/default.css"]
    }
  ]
}