{
  "background": {
    "scripts": [
      "lib/config.js",
      "lib/chrome.js",
      "lib/runtime.js",
      "lib/common.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "data/icons/128.png",
      "64": "data/icons/64.png"
    },
    "default_popup": "data/popup/popup.html",
    "default_title": "Audio Equalizer (VLC)"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "data/content_script/inject.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Stylish audio equalizer in your browser's toolbar popup, with VLC presets.",
  "icons": {
    "128": "data/icons/128.png",
    "64": "data/icons/64.png"
  },
  "manifest_version": 2,
  "name": "Audio Equalizer (VLC)",
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "contextMenus"
  ],
  "version": "0.2.3"
}