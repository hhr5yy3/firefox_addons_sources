{
  "name": "Cat-In-Tab",
  "description": "A cat that can walk around on your page",
  "version": "1.5",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.min.js"
    ]
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "logo/16x16.png",
    "24": "logo/24x24.png",
    "32": "logo/32x32.png",
    "48": "logo/48x48.png",
    "64": "logo/64x64.png",
    "128": "logo/128x128.png",
    "256": "logo/256x256.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "logo/16x16.png",
      "24": "logo/24x24.png",
      "32": "logo/32x32.png",
      "48": "logo/48x48.png",
      "64": "logo/64x64.png",
      "128": "logo/128x128.png",
      "256": "logo/256x256.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.min.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "sprites/*.png",
    "main.min.js",
    "main.min.css"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "alicorndev@test.com"
    }
  }
}