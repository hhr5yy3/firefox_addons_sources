{
  "manifest_version": 2,
  "name": "Speed Dial",
  "version": "1.2.0",
  "default_locale": "en",
  "description": "__MSG_description__",
  "icons": {
    "64": "img/64.png"
  },
  "applications": {
    "gecko": {
      "id": "@speed-dial-fast"
    }
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "history",
    "unlimitedStorage",
    "contextMenus",
    "storage",
    "topSites",
    "activeTab",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "pages/newtab.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Yahoo",
      "search_url": "https://search.yahoo.com/yhs/search?hspart=foxload&hsimp=yhs-speeddial&grd=1&p={searchTerms}",
      "keyword": "www",
      "favicon_url": "https://www.addonsearch.net/favicon.ico",
      "is_default": true
    }
  },
  "browser_action": {
    "default_icon": "img/64.png"
  }
}