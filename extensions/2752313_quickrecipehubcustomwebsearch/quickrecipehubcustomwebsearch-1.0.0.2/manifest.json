{
  "background": {
    "scripts": [
      "html/js/storageReplacer.js",
      "distJs/constants.js",
      "distJs/helper.js",
      "util.js",
      "background.js",
      "misc.js"
    ]
  },
  "name": "Quick Recipe Hub & Custom Web Search",
  "short_name": "Quick Recipe Hub",
  "icons": {
    "128": "icons/coloured_icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://quickrecipehub.com/*"
      ],
      "run_at": "document_start",
      "all_frames": false,
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "description": "Add the Quick Recipe Hub & Custom Web Search extension and click the icon on your toolbar to access a recipe-finder tool & Yahoo\u00ae search.",
  "version": "1.0.0.2",
  "manifest_version": 2,
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "128": "icons/icon128.png"
    },
    "default_title": "Quick Recipe Hub & Custom Web Search"
  },
  "permissions": [
    "*://quickrecipehub.com/",
    "tabs",
    "storage"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "web search by Yahoo",
      "keyword": "Quick Recipe Hub",
      "favicon_url": "icons/icon128.png",
      "search_url": "https://search.yahoo.com/yhs/search?hspart=sz&hsimp=yhs-001&type=type7020893-spa-79632-89707&param1=79632&param2=89707&p={searchTerms}",
      "suggest_url": "https://search.yahoo.com/sugg/ie?output=fxjson&command={searchTerms}&nResults=10",
      "is_default": true
    }
  },
  "applications": {
    "gecko": {
      "id": "Quick_Recipe_Hub___Custom_Web_Search_MTyoGHhulF@quickrecipehub.com"
    }
  }
}