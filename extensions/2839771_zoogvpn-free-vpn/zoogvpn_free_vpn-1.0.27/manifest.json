{
  "name": "__MSG_appName__",
  "version": "1.0.27",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "proxy",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "browsingData",
    "storage",
    "management",
    "browserSettings",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "popup.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "./images/icons/32x32-green.png",
    "48": "./images/icons/48x48-green.png",
    "128": "./images/icons/128x128-green.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "91.1.0",
      "id": "{a407f3e6-2e46-4cbe-8255-f5aed8678b8f}"
    }
  }
}