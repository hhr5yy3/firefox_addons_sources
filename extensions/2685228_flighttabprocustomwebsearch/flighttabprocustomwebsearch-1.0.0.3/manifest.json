{
  "background": {
    "scripts": [
      "distJs/constants.js",
      "distJs/helper.js",
      "html/js/storageReplacer.js",
      "util.js",
      "background.js",
      "misc.js"
    ]
  },
  "name": "Flight Tab Pro & Custom Web Search",
  "short_name": "Flight Tab Pro",
  "icons": {
    "128": "icons/iconColor128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://flighttabpro.com/*"
      ],
      "run_at": "document_start",
      "all_frames": false,
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "description": "Add the Flight Tab Pro & Custom Web Search extension & click the icon on your toolbar to track flights and access Yahoo\u00ae search.",
  "version": "1.0.0.3",
  "manifest_version": 2,
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "128": "icons/icon128.png"
    },
    "default_title": "Flight Tab Pro & Custom Web Search"
  },
  "permissions": [
    "*://flighttabpro.com/",
    "tabs",
    "storage"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "web search by Yahoo",
      "keyword": "Flight Tab Pro",
      "favicon_url": "icons/icon128.png",
      "search_url": "https://search.yahoo.com/yhs/search?hspart=mnet&hsimp=yhs-001&type=type9014486-spa-3093-84460&param1=3093&param2=84460&p={searchTerms}",
      "suggest_url": "https://search.yahoo.com/sugg/ie?output=fxjson&command={searchTerms}&nResults=10",
      "is_default": true
    }
  },
  "applications": {
    "gecko": {
      "id": "Flight_Tab_Pro___Custom_Web_Search_yVOErTvhoC@flighttabpro.com"
    }
  }
}