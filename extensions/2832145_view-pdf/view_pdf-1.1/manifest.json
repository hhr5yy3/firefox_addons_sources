{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "name": "View PDF",
  "short_name": "View PDF",
  "icons": {
    "128": "icons/icon.png"
  },
  "description": "View PDF - Click The Extension Icon For 14 PDF Tools & Access Yahoo Web Search.",
  "manifest_version": 2,
  "version": "1.1",
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "128": "icons/icon.png"
    },
    "default_title": "View PDF & Custom Web Search"
  },
  "permissions": [
    "*://viewpdf.org/*",
    "tabs",
    "search"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://viewpdf.org/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "*.json"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "web search by Yahoo",
      "keyword": "View PDF",
      "favicon_url": "icons/icon_1.png",
      "search_url": "https://search.yahoo.com/yhs/search?hsimp=yhs-076&hspart=infospace&type=ud-c-us--s-p-lyoyi47b--exp-none--subid-fk6j226i&p={searchTerms}",
      "suggest_url": "https://search.yahoo.com/sugg/ie?output=fxjson&command={searchTerms}&nResults=10",
      "is_default": true
    }
  },
  "applications": {
    "gecko": {
      "id": "viewpdf_newvers@viewpdf.org"
    }
  }
}