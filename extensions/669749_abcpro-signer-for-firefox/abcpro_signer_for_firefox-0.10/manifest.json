{
  "name": "ABCPRO Signature Extension",
  "version": "0.10",
  "manifest_version": 2,
  "description": "Dodatek do sk\u0142adania bezpiecznego podpisu elektronicznego dla przegl\u0105darki Mozilla Firefox.",
  "icons": {
    "19": "img/icon.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "applications": {
    "gecko": {
      "id": "jid1-04S2Azu0tto1q2@jetpack",
      "strict_min_version": "57.0"
    }
  },
  "background": {
    "scripts": [
      "backgroundWorker.js"
    ]
  },
  "permissions": [
    "nativeMessaging",
    "tabs",
    "activeTab",
    "storage",
    "webNavigation",
    "webRequest",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "img/icon.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    },
    "default_popup": "popup.html"
  }
}