{
  "description": "Automatically enables Picture-in-Picture for all web videos.",
  "manifest_version": 2,
  "name": "Enable Picture-in-Picture",
  "short_name": "Enable PiP",
  "version": "1.0",
  "homepage_url": "https://www.svenbader.de/",
  "icons": {
    "48": "icons/48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "enablepictureinpicture.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{31a4c81b-add0-4ce4-b6e4-b54dcb0f4d1b}"
    }
  }
}