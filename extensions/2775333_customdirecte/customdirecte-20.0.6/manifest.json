{
  "name": "CustomDirecte",
  "description": "Ajoute de nouvelles options sur EcoleDirecte ! Afficher votre moyenne g\u00e9n\u00e9rale, personnaliser l'interface...",
  "author": "Bastian NOEL",
  "version": "20.0.6",
  "manifest_version": 2,
  "devtools_page": "/pages/popup/interface.html",
  "permissions": [
    "storage",
    "*://*.ecoledirecte.com/*"
  ],
  "browser_action": {
    "default_title": "CustomDirecte",
    "default_icon": {
      "16": "/icons/default/icon16.png",
      "32": "/icons/default/icon32.png",
      "48": "/icons/default/icon48.png",
      "128": "/icons/default/icon128.png"
    }
  },
  "icons": {
    "16": "/icons/default/icon16.png",
    "32": "/icons/default/icon32.png",
    "48": "/icons/default/icon48.png",
    "128": "/icons/default/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.ecoledirecte.com/*"
      ],
      "run_at": "document_start",
      "all_frames": true,
      "js": [
        "/scripts/main.js"
      ],
      "css": [
        "/styles/notesTable.css",
        "/styles/sidebar.css",
        "/styles/customizationButton.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "/styles/customizations.css",
    "/pages/popup/interface.html",
    "/scripts/darkreader.js",
    "/scripts/tippy.js",
    "/styles/default.css",
    "/icons/default/icon.svg",
    "/icons/EcoleDirecte/default.ico",
    "/icons/EcoleDirecte/gold.ico",
    "/icons/EcoleDirecte/magenta.ico",
    "/icons/EcoleDirecte/purple.ico",
    "/icons/EcoleDirecte/turquoise.ico"
  ],
  "background": {
    "scripts": [
      "/scripts/background.js"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{1c8331de-d996-453d-b08a-463ca9588149}"
    }
  }
}