{
  "manifest_version": 2,
  "name": "Hostname in title",
  "version": "1.1resigned1",
  "description": "Puts the hostname of the tab in the title",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "hostname.js"
      ],
      "run_at": "document_end"
    }
  ],
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "tabs",
    "*://*/*",
    "storage"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{b8842ade-9efd-4461-84bd-2106e91854fe}"
    }
  }
}