{
  "manifest_version": 2,
  "name": "S\u00e9neca F\u00e1cil",
  "version": "0.0.5.1",
  "description": "Mejore la experiencia de usuario en el cuaderno de S\u00e9neca",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "S\u00e9neca F\u00e1cil",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://seneca.juntadeandalucia.es/seneca/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "commands": {
    "open-udemy": {
      "suggested_key": {
        "default": "Ctrl+Shift+U",
        "mac": "Command+Shift+U"
      },
      "description": "Activa algunas funciones de prueba"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{db58c3fc-a673-4f94-a786-bb30a01fcc94}"
    }
  }
}