{
    "applications": {
        "gecko": {
            "id": "intgcal@egarracingteam.com.ar"
        }
    },
    "manifest_version": 2,
    "name": "Pinned Google Calendar",
    "version": "2.2.0",
    "description": "Go to the pinned Google Calendar tab. If it doesn't exist, it's created.",
    "icons": {
        "48": "icons/gcalendar-48.png",
        "64": "icons/gcalendar-64.png"
    },
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_icon": {
            "32": "icons/gcalendar-32.png"
        },
        "default_title": "Pinned Google Calendar",
        "default_area": "navbar"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+Shift+C"
            }
        }
    },
    "options_ui": {
        "page": "options/options.html",
        "browser_style": true
    }
}