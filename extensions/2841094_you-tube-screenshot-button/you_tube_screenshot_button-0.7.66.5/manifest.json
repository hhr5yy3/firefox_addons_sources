{
  "manifest_version": 2,
  "short_name": "__MSG_Abbr__",
  "name": "__MSG_appName__",
  "description": "__MSG_appDescription__",
  "version": "0.7.66.5",
  "default_locale": "en",
  "icons": {
    "32": "icon32.png",
    "64": "icon64.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon64.png",
    "default_title": "__MSG_browserActionDefaultTitle__"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content_script.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{6fa6f9a3-c8d8-45b4-8d35-d1fe9b5f5d51}"
    }
  }
}