{
  "name": "Locoloader",
  "description": "Add extra powers\u26a1 to Locoloader.com!",
  "version": "1.6.8",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "downloads",
    "declarativeNetRequest",
    "storage",
    "management",
    "*://*.cdn13.com/*",
    "*://*.xhcdn.com/*",
    "*://*.ahcdn.com/*",
    "*://*.thefantazy.com/*",
    "*://*.hdsex.org/*",
    "*://*.kompoz2.com/*",
    "*://*.porntrex.com/*",
    "*://*.nhentai.net/*",
    "*://*.fansly.com/*",
    "*://*.onlyfans.com/*",
    "*://*.zype.com/*",
    "*://*.audiomack.com/*",
    "*://*.deviantart.com/*",
    "*://*.tiktok.com/*",
    "*://*.tiktokcdn.com/*",
    "https://www.locoloader.com/*",
    "https://www.locoloader.test/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.locoloader.com/*",
        "https://www.locoloader.test/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{21368c2e-1e43-414c-9c63-e1b87782681f}"
    }
  }
}