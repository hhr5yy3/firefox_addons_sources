{
  "manifest_version": 3,
  "version": "4.1.0",
  "name": "Fake Filler",
  "short_name": "Fake Filler",
  "description": "A form filler that fills all inputs on a page with fake/dummy data.",
  "default_locale": "en",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "64": "images/icon-64.png",
    "96": "images/icon-96.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "64": "images/icon-64.png",
      "96": "images/icon-96.png",
      "128": "images/icon-128.png"
    },
    "default_title": "Fill All Inputs with Dummy Data"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage",
    "scripting"
  ],
  "background": {
    "scripts": [
      "src/service-worker.js"
    ],
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content-script.js"
      ],
      "all_frames": true
    }
  ],
  "options_ui": {
    "page": "index.html",
    "open_in_tab": true
  },
  "commands": {
    "fill_all_inputs": {
      "description": "Fill all inputs"
    },
    "fill_this_form": {
      "description": "Fill this form"
    },
    "fill_this_input": {
      "description": "Fill this input"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{7efbd09d-90ad-47fa-b91a-08c472bdf566}"
    }
  }
}