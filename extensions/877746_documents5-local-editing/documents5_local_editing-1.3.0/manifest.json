{
  "manifest_version": 2,
  "name": "Documents5 Local Editing",
  "version": "1.3.0",
  "version_name": "1.3.0",
  "description": "Addon for otris Documents5 to edit files locally.",
  "author": "otris",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "applications": {
    "gecko": {
      "id": "DOCUMENTSLocalEditing@otris.de",
      "strict_min_version": "54.0"
    }
  },
  "page_action": {
    "default_icon": {
      "1024": "icons/doc_1024.png",
      "512": "icons/doc_512.png",
      "256": "icons/doc_256.png",
      "128": "icons/doc_128.png",
      "64": "icons/doc_64.png",
      "32": "icons/doc_32.png",
      "16": "icons/doc_16.png"
    },
    "default_popup": "options.html",
    "default_title": "Local Editing Options"
  },
  "icons": {
    "1024": "icons/doc_1024.png",
    "512": "icons/doc_512.png",
    "256": "icons/doc_256.png",
    "128": "icons/doc_128.png",
    "64": "icons/doc_64.png",
    "32": "icons/doc_32.png",
    "16": "icons/doc_16.png"
  },
  "permissions": [
    "nativeMessaging",
    "tabs",
    "cookies",
    "*://*/*"
  ]
}