{
  "manifest_version": 2,
  "name": "__MSG_EXTENSION_NAME__",
  "short_name": "__MSG_EXTENSION_SHORT_NAME__",
  "version": "2.30.5",
  "homepage_url": "https://alihelper.net",
  "description": "__MSG_EXTENSION_DESCRIPTION__",
  "default_locale": "en",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "AliHelper",
    "default_icon": "16.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "settings.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.alihelper.net/*"
      ],
      "js": [
        "js/jquery.js",
        "site_button.js"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "*://*.aliexpress.com/*",
        "*://*.aliexpress.ru/*",
        "*://*.aliexpress.us/*",
        "*://*.tmall.ru/*"
      ],
      "css": [
        "css/icomoon.css",
        "css/_fonts.min.css",
        "css/magnific-popup.css",
        "css/_alihelper-app.css",
        "css/_alihelper-common.css"
      ],
      "js": [
        "js/jquery.js",
        "js/jquery-ui.min.js",
        "js/jquery.magnific-popup.min.js",
        "content-script.js",
        "alihelper-app.js",
        "alihelper-common.js"
      ]
    }
  ],
  "permissions": [
    "notifications",
    "http://*/*",
    "https://*/*",
    "cookies",
    "storage",
    "tabs",
    "webNavigation"
  ],
  "web_accessible_resources": [
    "images/*",
    "fonts/*",
    "config/*",
    "16.png",
    "48.png",
    "128.png"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{333f4540-f467-419b-8410-233078ae8813}"
    }
  }
}