{
  "manifest_version": 3,
  "default_locale": "en",
  "name": "Carta",
  "version": "2.1.17",
  "description": "__MSG_extensionDescription__",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.iife.js"
    ],
    "type": "module"
  },
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": "cartaLogo.png"
  },
  "icons": {
    "16": "cartaLogo.png",
    "48": "cartaLogo.png",
    "128": "cartaLogo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "content/index.iife.js"
      ]
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "content-ui/index.iife.js"
      ]
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "css": [
        "content-ui/style.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "*.js",
        "*.css",
        "*.svg",
        "*.txt"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{8eaa1bcc-471b-11ee-be56-0242ac120002}"
    }
  }
}