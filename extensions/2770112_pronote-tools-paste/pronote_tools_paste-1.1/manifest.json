{
  "manifest_version": 2,
  "name": "Pronote Tools",
  "description": "Outils pour faire copier coller des notes",
  "version": "1.1",
  "icons": {
    "128": "128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardRead",
    "storage",
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "js": [
        "popup.html"
      ],
      "matches": [
        "http://dev.local/*"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{7e251d6d-9c40-42c7-be5c-76dd77c5fa34}"
    }
  }
}