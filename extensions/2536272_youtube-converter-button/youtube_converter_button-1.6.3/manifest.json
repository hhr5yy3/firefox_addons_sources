{
  "manifest_version": 2,
  "name": "Y2mate.com - YouTube Converter & Downloader",
  "version": "1.6.3",
  "homepage_url": "https://www.y2mate.com/",
  "description": "Y2mate.com - Free YouTube Converter Button helps you save YouTube video and audio files just in 1 click",
  "icons": {
    "32": "icons/icon-32.png",
    "96": "icons/icon-96.png"
  },
  "permissions": [
    "https://www.y2mate.com/youtube-converter",
    "https://www.y2mate.com/youtube-to-mp3",
    "https://www.y2mate.com/",
    "webRequest",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*",
        "*://youtube.com/*",
        "*://youtu.be/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{8f4bbf79-5514-4d04-a901-d5fabfe91d73}"
    }
  }
}