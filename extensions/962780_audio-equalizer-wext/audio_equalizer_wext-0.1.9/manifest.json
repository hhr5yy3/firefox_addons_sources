{
  "version": "0.1.9",
  "manifest_version": 3,
  "name": "Audio Equalizer",
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "storage",
    "contextMenus"
  ],
  "homepage_url": "https://mybrowseraddon.com/audio-equalizer.html",
  "description": "Stylish audio equalizer in your browser's toolbar popup.",
  "commands": {
    "_execute_action": {}
  },
  "web_accessible_resources": [
    {
      "matches": [
        "*://*/*"
      ],
      "resources": [
        "data/content_script/page_context/*"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "109.0",
      "id": "{63d150c4-394c-4275-bc32-c464e76a891c}"
    }
  },
  "background": {
    "scripts": [
      "lib/config.js",
      "lib/chrome.js",
      "lib/runtime.js",
      "lib/common.js"
    ]
  },
  "action": {
    "default_title": "Audio Equalizer",
    "default_popup": "data/popup/popup.html",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_start",
      "js": [
        "data/content_script/inject.js"
      ]
    }
  ],
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}