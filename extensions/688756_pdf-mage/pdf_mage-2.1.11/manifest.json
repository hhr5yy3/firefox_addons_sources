{
  "name": "PDF Mage",
  "version": "2.1.11",
  "manifest_version": 2,
  "description": "Click to save page as PDF",
  "browser_action": {
    "default_icon": "images/icon_16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "applications": {
    "gecko": {
      "id": "jid1-GeRCnsiDhZiTvA@jetpack"
    }
  },
  "options_ui": {
    "page": "options.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+P"
      }
    }
  },
  "permissions": [
    "tabs",
    "downloads",
    "storage",
    "activeTab",
    "contextMenus",
    "<all_urls>"
  ]
}