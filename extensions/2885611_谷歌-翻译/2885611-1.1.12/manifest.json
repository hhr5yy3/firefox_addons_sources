{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "description": "__MSG_description__",
  "version": "1.1.12",
  "default_locale": "zh_CN",
  "short_name": "Google Translate",
  "icons": {
    "128": "icons/icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "code/selection.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "code/pref.js"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{b5571d6c-fd1a-5901-8976-6c5582ecc83c}"
    }
  },
  "permissions": [
    "declarativeNetRequest",
    "storage",
    "contextMenus"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icons/icon.png",
      "38": "icons/icon.png"
    }
  }
}