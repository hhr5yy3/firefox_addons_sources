{
  "version": "0.1.3",
  "manifest_version": 2,
  "short_name": "open-in-pdf",
  "name": "Open in PDF Viewer",
  "background": {
    "page": "lib/chrome/background.html"
  },
  "description": "Easily open desired links in PDF viewer",
  "homepage_url": "http://mybrowseraddon.com/open-in-pdf.html",
  "permissions": [
    "storage",
    "downloads",
    "contextMenus",
    "notifications",
    "nativeMessaging"
  ],
  "options_ui": {
    "open_in_tab": true,
    "page": "data/options/options.html"
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{2582ab30-4fca-475f-88d0-c1a9b9ed978f}"
    }
  }
}