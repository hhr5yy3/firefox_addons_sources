{
  "manifest_version": 2,
  "name": "Copy Equation from ChatGPT and Wikipedia",
  "version": "3.6",
  "description": "Copy MathML and LaTeX from math elements",
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "svg/*.svg",
    "css/*.css",
    "popup.html"
  ],
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://chatgpt.com/*",
        "https://*.wikipedia.org/*"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{f42db7dd-558e-4cbb-89c1-c60f28aba1df}"
    }
  }
}