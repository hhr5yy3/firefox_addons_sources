{
  "manifest_version": 2,
  "name": "SD Downloader",
  "description": "Easily download publicated files for private copies from Studydrive. This extension is not affiliated with or endorsed by Studydrive.",
  "version": "1.0",
  "permissions": [
    "cookies",
    "tabs",
    "*://*.studydrive.net/*",
    "https://capture.artifactpowered.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{4ae86830-bdb1-4232-b6b8-654ad750b6e3}"
    }
  }
}