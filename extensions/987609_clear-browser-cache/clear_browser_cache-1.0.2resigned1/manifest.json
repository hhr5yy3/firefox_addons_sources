{
  "manifest_version": 2,
  "name": "Clear Cache",
  "description": "Clear Cache On One Cilck",
  "version": "1.0.2resigned1",
  "author": "Guley",
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "64": "icon/64.png",
    "128": "icon/128.png"
  },
  "browser_action": {
    "default_title": "Clear Cache",
    "default_icon": {
      "16": "icon/16.png",
      "32": "icon/32.png",
      "64": "icon/64.png",
      "128": "icon/128.png"
    }
  },
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "permissions": [
    "notifications",
    "browsingData",
    "storage"
  ],
  "commands": {
    "toggle-feature": {
      "suggested_key": {
        "default": "Ctrl+Shift+C",
        "linux": "Ctrl+Shift+C"
      },
      "description": "Clear Cache"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{a2998f0a-3e99-457d-8cae-45382f33ceff}"
    }
  }
}