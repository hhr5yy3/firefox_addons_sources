{
  "manifest_version": 2,
  "name": "Crop",
  "description": "Adds a context menu entry to crop an image.",
  "version": "0.1.1",
  "homepage_url": "https://github.com/innocenzi/crop-extension",
  "icons": {
    "16": "icons/crop.svg",
    "32": "icons/crop.svg",
    "48": "icons/icon.svg",
    "96": "icons/icon.svg"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ]
}