{
  "manifest_version": 2,
  "name": "__MSG_name__",
  "short_name": "__MSG_shortName__",
  "version": "1.2",
  "description": "__MSG_description__",
  "default_locale": "en",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://storage.googleapis.com/biggest_bucket/annotations/*",
    "storage"
  ],
  "optional_permissions": [
    "downloads"
  ],
  "browser_action": {
    "default_title": "Annotations Restored",
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.youtube.com/*"
      ],
      "js": [
        "js/annotationlib/dist/AnnotationParser.js",
        "js/annotationlib/dist/AnnotationRenderer.js",
        "js/content.js"
      ],
      "css": [
        "js/annotationlib/dist/AnnotationRenderer.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{0731d555-4732-4047-99f9-38a388ffa044}"
    }
  }
}