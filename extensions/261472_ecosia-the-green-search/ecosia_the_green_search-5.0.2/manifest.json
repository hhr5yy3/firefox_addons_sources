{
  "name": "__MSG_store_title_new__",
  "description": "__MSG_store_shortdesc_firefox__",
  "version": "5.0.2",
  "default_locale": "en",
  "homepage_url": "https://www.ecosia.org",
  "icons": {
    "16": "img/favicon16.png",
    "48": "img/logo_square_transparent48.png",
    "128": "img/logo_square_transparent128.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "img/favicon16.png",
    "img/logo.png"
  ],
  "browser_action": {
    "default_title": "__MSG_browser_action_hover__",
    "default_icon": "img/icon24.png",
    "browser_style": false
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.ecosia.org/*"
      ],
      "js": [
        "funnel.js"
      ],
      "run_at": "document_start"
    }
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Ecosia search",
      "keyword": "@ecosia",
      "search_url": "https://www.ecosia.org/search?q={searchTerms}&addon=firefox&addonversion=5.0.2&method=topbar",
      "favicon_url": "https://www.ecosia.org/static/icons/favicon.ico",
      "suggest_url": "https://ac.ecosia.org/?q={searchTerms}&type=list&mkt=__MSG_@@ui_locale__",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "applications": {
    "gecko": {
      "id": "{d04b0b40-3dab-4f0b-97a6-04ec3eddbfb0}",
      "strict_min_version": "65.0"
    }
  },
  "permissions": [
    "*://*.ecosia.org/*",
    "storage",
    "contextMenus"
  ]
}