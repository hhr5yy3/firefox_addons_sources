{
  "manifest_version": 2,
  "name": "Cupom de Desconto em lojas online",
  "description": "Com o Cupom Desconto voc\u00ea n\u00e3o perde tempo procurando cupons e descontos na internet em suas lojas preferidas.",
  "author": "",
  "permissions": [
    "<all_urls>",
    "webRequest",
    "tabs"
  ],
  "icons": {
    "128": "img/cupomdesconto.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "version": "3.0.5",
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}