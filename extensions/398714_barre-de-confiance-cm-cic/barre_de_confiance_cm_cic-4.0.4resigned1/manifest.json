{
  "name": "Barre de Confiance CM-CIC",
  "version": "4.0.4resigned1",
  "manifest_version": 2,
  "description": "Aide \u00e0 d\u00e9tecter les sites de confiance CM, CIC et les tentatives de phishing",
  "browser_action": {
    "default_icon": "images/bc_inet.gif",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "confiance/domaineconfiance.js",
      "blocage/listenoire.js",
      "storage/localstorage.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "cookies"
  ],
  "homepage_url": "https://www.creditmutuel.fr/",
  "icons": {
    "32": "images/bc_logo.png"
  },
  "web_accessible_resources": [
    "blocage/blocage.css",
    "blocage/blocage.js",
    "blocage/bc_logo-detective.png"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{75493B06-1504-4976-9A55-B6FE240FF0BF}"
    }
  }
}