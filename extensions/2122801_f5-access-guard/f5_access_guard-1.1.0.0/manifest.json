{
  "name": "F5 Access Guard",
  "version": "1.1.0.0",
  "description": "F5 Access Guard enables seamless, secure access to enterprise applications",
  "icons": {
    "32": "32x32.png",
    "64": "64x64.png",
    "128": "128x128.png"
  },
  "permissions": [
    "idle",
    "webRequest",
    "nativeMessaging",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "common-utilities.js",
      "globals.js",
      "clientinfo-status-tracker.js",
      "webrequest.js",
      "nmapi-common.js",
      "nmapi.js",
      "background.js"
    ]
  },
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "f5_access_guard@f5.com",
      "strict_min_version": "50.0"
    }
  }
}
