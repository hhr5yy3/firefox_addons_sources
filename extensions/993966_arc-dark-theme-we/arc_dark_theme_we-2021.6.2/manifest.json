{
  "manifest_version": 2,

  "applications": {
    "gecko": {
      "id": "arc-dark-theme@afnankhan",
      "strict_min_version": "89.0"
    }
  },

  "name": "Arc Dark Theme",
  "version": "2021.6.2",
  "description": "Arc Dark theme for Firefox",

  "theme": {
    "colors": {
      "frame": "#2F343F",
      "frame_inactive": "#313742",

      "tab_text": "#D3DAE3",
      "tab_line": "rgba(0, 0, 0, 0)",
      "tab_loading": "#5294E2",
      "tab_selected": "#4b5162",
      "tab_background_text": "#AFB8C6",
      "tab_background_separator": "rgba(255, 255, 255, 0.5)",

      "icons": "#D3DAE3",
      "icons_attention": "#5294E2",

      "toolbar": "#383C4A",
      "toolbar_top_separator": "#272A34",
      "toolbar_bottom_separator": "#2B2E39",
      "toolbar_vertical_separator": "#2B2E39",

      "toolbar_field": "#404552",
      "toolbar_field_text": "#D3DAE3",
      "toolbar_field_border": "#2B2E39",
      "toolbar_field_separator": "rgba(255,255,255,0.2)",

      "toolbar_field_focus": "#404552",
      "toolbar_field_border_focus": "#5294E2",

      "toolbar_field_highlight": "#5294E2",
      "toolbar_field_highlight_text": "#FFFFFF",

      "button_background_hover": "#4b5162",
      "button_background_active": "#5294E2",

      "bookmark_text": "#D3DAE3",

      "popup": "#404552",
      "popup_text": "#D3DAE3",
      "popup_border": "#2B2E39",
      "popup_highlight": "#5294E2",
      "popup_highlight_text": "#FFFFFF",

      "sidebar": "#383C4A",
      "sidebar_text": "#D3DAE3",
      "sidebar_border": "#2B2E39",
      "sidebar_highlight": "#5294E2",
      "sidebar_highlight_text": "#FFFFFF",

      "ntp_text": "#D3DAE3",
      "ntp_background": "#404552"
    }
  }
}
