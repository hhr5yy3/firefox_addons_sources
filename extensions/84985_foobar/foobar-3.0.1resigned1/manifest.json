{
  "manifest_version": 2,
  "name": "Foobar",
  "description": "Easy select-a-search and multi-search",
  "version": "3.0.1resigned1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "omnibox": {
    "keyword": "foo"
  },
  "icons": {
    "16": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "idle"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "foobar@unnecessarilylongurl.com"
    }
  }
}