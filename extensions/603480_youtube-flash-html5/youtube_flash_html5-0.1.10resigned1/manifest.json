{
  "version": "0.1.10resigned1",
  "manifest_version": 2,
  "short_name": "youtube-flash-html",
  "name": "Flash-HTML5 for YouTube\u2122",
  "background": {
    "page": "lib/chrome/background.html"
  },
  "description": "Play YouTube Videos in Flash or HTML5 Player",
  "homepage_url": "http://mybrowseraddon.com/youtube-flash-html.html",
  "permissions": [
    "storage",
    "*://*.mybrowseraddon.com/*",
    "*://*.youtube.com/*"
  ],
  "browser_action": {
    "default_popup": "data/popup/popup.html",
    "default_title": "Flash-HTML5 for YouTube\u2122",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "run_at": "document_start",
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "data/communication.js",
        "data/content_script/inject.js"
      ]
    }
  ],
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "jid1-o2qEVrZ4t5FJWu@jetpack"
    }
  }
}