{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "short_name": "__MSG_extensionDescription__",
  "default_locale": "en",
  "version": "3.5.0",
  "browser_specific_settings": {
    "gecko": {
      "id": "jid1-KKzOGWgsW3Ao4Q@jetpack",
      "strict_min_version": "56.0"
    },
    "gecko_android": {
      "id": "jid1-KKzOGWgsW3Ao4Q@jetpack",
      "strict_min_version": "78.0"
    }
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "author": "Daniel Kladnik @ kiboke studio",
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "webNavigation"
  ],
  "background": {
    "scripts": [
      "data/rules.js",
      "data/context-menu.js"
    ]
  },
  "options_ui": {
    "page": "data/options.html",
    "browser_style": true
  },
  "browser_action": {
    "default_popup": "data/menu/index.html",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png"
    }
  }
}