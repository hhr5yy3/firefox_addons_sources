{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "browser_style": false,
    "default_icon": "icon.png",
    "default_title": "Video Downloader Professional"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "all_frames": false,
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "icons": {
    "16": "icon.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "incognito": "spanning",
  "name": "__MSG_appName__",
  "short_name": "DLA",
  "options_ui": {
    "page": "settings.html"
  },
  "permissions": [
    "downloads",
    "notifications",
    "*://*/*",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "storage",
    "management",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.98.3resigned1",
  "web_accessible_resources": [],
  "browser_specific_settings": {
    "gecko": {
      "id": "alldownloader@link64"
    }
  }
}