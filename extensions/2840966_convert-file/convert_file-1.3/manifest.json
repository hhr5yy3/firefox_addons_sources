{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "name": "Convert File",
  "short_name": "Convert File",
  "icons": {
    "128": "icons/Asset_1.png"
  },
  "description": "Convert File & Custom Web Search extension then click the icon on your toolbar Convert Your Files plus Yahoo Web Search.",
  "manifest_version": 2,
  "version": "1.3",
  "browser_action": {
    "browser_style": false,
    "default_icon": {
      "128": "icons/Asset_1.png"
    },
    "default_title": "Convert File"
  },
  "permissions": [
    "*://convertfile.ai/*",
    "tabs",
    "search",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://convertfile.ai/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "*.json"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "web search by Yahoo",
      "keyword": "Convert File",
      "favicon_url": "icons/icon_1.png",
      "search_url": "https://search.yahoo.com/yhs/search?hsimp=yhs-081&hspart=infospace&p={searchTerms}&grd=1",
      "suggest_url": "https://search.yahoo.com/sugg/ie?output=fxjson&command={searchTerms}&nResults=10",
      "is_default": true
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "convertfile@convertfile.ai"
    }
  }
}