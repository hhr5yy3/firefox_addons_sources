{
  "name": "Faster Panopto",
  "version": "0.7.0",
  "description": "Forked version of Fast panopto designed to work on both firefox and chrome",
  "homepage_url": "https://github.com/chazzox/faster-panopto/",
  "background": {
    "scripts": [
      "background.818ea696.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "webRequest",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.3e8905c2.html",
    "browser_style": true,
    "default_icon": {
      "32": "images/disabled.png"
    }
  },
  "icons": {
    "32": "images/enabled.png"
  },
  "web_accessible_resources": [
    "images/arrow.png",
    "images/disabled.png",
    "images/enabled.png"
  ],
  "manifest_version": 2,
  "browser_specific_settings": {
    "gecko": {
      "id": "{f0e4c52c-f1ce-4a8f-b904-675cbdb253e6}"
    }
  }
}