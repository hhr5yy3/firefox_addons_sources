{
  "manifest_version": 2,
  "name": "KingdomLikes.com addon",
  "version": "5.2",
  "description": "Check interaction button",
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "*://*.youtube.com/*",
    "*://*.kingdomlikes.com/*",
    "*://*.facebook.com/*",
    "*://*.twitter.com/*",
    "*://*.instagram.com/*",
    "*://*.pinterest.com/*",
    "*://*.tiktok.com/*"
  ],
  "icons": {
    "48": "icons/kingdomlikes.png",
    "96": "icons/kingdomlikes@2x.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/kingdomlikes.png"
  },
  "content_scripts": [
    {
      "exclude_matches": [
        "*://*.youtube.com/watch*"
      ],
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "youtubesub.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.youtube.com/watch*",
        "*://*.youtube.com/service_ajax*"
      ],
      "js": [
        "youtubelike.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "facebooklike.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.instagram.com/*"
      ],
      "js": [
        "instagramfollow.js",
        "instagramlike.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.kingdomlikes.com/*"
      ],
      "js": [
        "executeInPage.js",
        "kingdomlikes.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.twitter.com/*"
      ],
      "js": [
        "twitterfollow.js",
        "twitterretweet.js",
        "twitterlike.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.tiktok.com/*"
      ],
      "js": [
        "tiktoklikes.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{7115e37b-c12a-445c-9d96-2b7f4748bb97}"
    }
  }
}