{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "default_locale": "fr",
  "author": "Fran√ßois Duprat",
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "storage",
    "cookies"
  ],
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/spoofer.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_extName__",
    "default_icon": {
      "32": "icons/32.png",
      "48": "icons/48.png"
    }
  },
  "web_accessible_resources": [
    "spoofer.js",
    "assets/**/*",
    "css/**/*"
  ],
  "version": "2.1.11",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "browser_specific_settings": {
    "gecko": {
      "id": "{edfc63b3-fc9b-4b6b-b9bf-4561ad548044}",
      "strict_min_version": "60.0"
    }
  }
}