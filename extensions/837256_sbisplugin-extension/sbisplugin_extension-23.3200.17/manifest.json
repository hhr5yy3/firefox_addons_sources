      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "icons": {
    "48": "sbisPlugin-logo-48.png",
    "128": "sbisPlugin-logo-128.png"
  }
}