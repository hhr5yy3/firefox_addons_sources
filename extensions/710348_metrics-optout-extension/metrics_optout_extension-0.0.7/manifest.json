{
  "manifest_version": 2,
  "name": "__MSG_title__",
  "description": "__MSG_description__",
  "default_locale": "ru",
  "icons": {
    "16": "icons/16.png",
    "19": "icons/19.png",
    "32": "icons/32.png",
    "38": "icons/38.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_title": "__MSG_title__",
    "default_popup": "popup.html",
    "browser_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "js": [
        "index.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "script.js"
  ],
  "author": "metrika.yndx@yandex.ru",
  "version": "0.0.7"
}
