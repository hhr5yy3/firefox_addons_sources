{
  "manifest_version": 2,
  "short_name": "__MSG_Abbr__",
  "name": "__MSG_appName__",
  "description": "__MSG_appDescription__",
  "version": "0.7.66.9",
  "default_locale": "en",
  "icons": {
    "32": "icon32.png",
    "64": "icon64.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon64.png",
    "default_title": "__MSG_browserActionDefaultTitle__"
  },
  "web_accessible_resources": [
    "/inject-scripts/*"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content_script.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{08aaa0e6-3ab6-4afd-9b94-319f19e096fa}"
    }
  }
}