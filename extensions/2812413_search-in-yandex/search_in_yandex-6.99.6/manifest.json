{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "6.99.6",
  "author": "Essence6996",
  "homepage_url": "https://github.com/Essence6996/YandexSearchEngineFirefox/",
  "default_locale": "en_US",
  "browser_specific_settings": {
    "gecko": {
      "id": "YandexSearchEngine@Essence6996"
    }
  },
  "icons": {
    "48": "icons/yandex.svg"
  },
  "permissions": [
    "menus",
    "storage",
    "search"
  ],
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "__MSG_extensionName__",
      "keyword": "@ya",
      "search_url": "https://__MSG_extensionTextUrl__/search/?text={searchTerms}"
    }
  }
}