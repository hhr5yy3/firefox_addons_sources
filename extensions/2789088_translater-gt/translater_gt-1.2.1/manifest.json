{
  "manifest_version": 3,
  "name": "__MSG_app_name__",
  "version": "1.2.1",
  "description": "__MSG_app_description__",
  "author": "chergav",
  "homepage_url": "https://github.com/chergav/translater",
  "default_locale": "en",
  "icons": {
    "16": "src/icons/16.png",
    "24": "src/icons/24.png",
    "32": "src/icons/32.png",
    "48": "src/icons/48.png",
    "128": "src/icons/128.png"
  },
  "action": {
    "default_title": "__MSG_app_name__",
    "default_popup": "src/popup/index.html",
    "default_icon": {
      "16": "src/icons/16.png",
      "24": "src/icons/24.png",
      "32": "src/icons/32.png",
      "48": "src/icons/48.png",
      "128": "src/icons/128.png"
    }
  },
  "commands": {
    "open-translater": {
      "suggested_key": {
        "default": "Ctrl+Shift+U",
        "mac": "Command+Shift+U"
      },
      "description": "__MSG_commands_open_translater__"
    }
  },
  "options_ui": {
    "page": "src/options/index.html",
