{
  "manifest_version": 2,
  "version": "0.2",
  "name": "VS Code for the Web",
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "icons": {
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  },
  "background": {
